            "blockIdx": 3
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "はぁ～っ、やったー！！\r\n目標達成っ！",
            "enText": "Pheeew! YEEEAH! I hit my goal!",
            "nextBlock": 5,
            "origClipLength": 57,
            "voiceIdx": 6,
            "voiceSheetId": "051027001",
            "animData": [
                {
                    "origLen": 68,
