            "origClipLength": 75,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "一生懸命で可愛かったぞ",
                    "enText": "You looked so cute dancing your heart out.",
                    "nextBlock": 5
                },
                {
                    "jpText": "一生懸命で可愛かったよ",
                    "enText": "You looked so cute dancing your heart out.",
                    "nextBlock": 5
