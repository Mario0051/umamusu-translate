            "origClipLength": 106,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": -1520353432539225388,
            "blockIdx": 1
        },
        {
            "jpName": "ユキノビジン",
            "enName": "Yukino Bijin",
            "jpText": "あっ、どうも～。",
