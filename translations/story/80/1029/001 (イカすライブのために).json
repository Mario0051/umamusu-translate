            "nextBlock": 4,
            "origClipLength": 93,
            "voiceIdx": 48,
            "voiceSheetId": "051029001",
            "animData": [
                {
                    "origLen": 105,
                    "pathId": 5929972641535531307
                },
                {
                    "origLen": 79,
                    "pathId": -3223006697561784954
                }
