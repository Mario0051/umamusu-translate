            "pathId": -8965612483844491444,
            "blockIdx": 8
        },
        {
            "jpName": "バンブーメモリー",
            "enName": "Bamboo Memory",
            "jpText": "ッ！　どうしたっスか！？",
            "enText": "What is it?!",
            "nextBlock": 10,
            "origClipLength": 60,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -9157193471249416593
