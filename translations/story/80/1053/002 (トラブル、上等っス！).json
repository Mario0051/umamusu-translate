            "pathId": -1551203812941350921,
            "blockIdx": 7
        },
        {
            "jpName": "風紀委員のウマ娘",
            "enName": "Disciplinary Committee Member",
            "jpText": "――た、大変です、委員長ッ！！",
            "enText": "Urgent news, Bamboo!",
            "nextBlock": 9,
            "origClipLength": 64,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 65,
                    "pathId": -2427368026226673240
                },
                {
                    "origLen": 75,
                    "pathId": -70924245404236870
