    "title": "プリンセス、殴る！",
    "enTitle": "Princess Punch!",
    "text": [
        {
            "jpName": "？？？",
            "enName": "???",
            "jpText": "どぉ～～――",
            "enText": "HAH!",
            "nextBlock": 2,
            "origClipLength": 52,
            "voiceIdx": -1,
