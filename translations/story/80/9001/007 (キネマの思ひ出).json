            ],
            "pathId": 5961256020364580626,
            "blockIdx": 2
        },
        {
            "jpName": "駿川たづな",
            "enName": "Tazuna Hayakawa",
            "jpText": "トレーナーさんトレーナーさん、\r\nご存じでしたかっ？",
            "enText": "Trainer! Trainer!",
            "nextBlock": 4,
            "origClipLength": 61,
            "voiceIdx": 55,
            "voiceSheetId": "059001001",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 2282337412475079174
