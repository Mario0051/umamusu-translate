            "blockIdx": 12
        },
        {
            "jpName": "駿川たづな",
            "enName": "Tazuna Hayakawa",
            "jpText": "だって……だって！！",
            "enText": "Well... You see...",
            "nextBlock": 14,
            "origClipLength": 51,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 50,
