            "pathId": 906765296483024244,
            "blockIdx": 19
        },
        {
            "jpName": "駿川たづな",
            "enName": "Tazuna Hayakawa",
            "jpText": "ここ、私の行きつけなんですよ。",
            "enText": "This is my go-to place!",
            "nextBlock": 21,
            "origClipLength": 54,
            "voiceIdx": 2,
            "voiceSheetId": "059001001",
            "animData": [
                {
                    "origLen": 68,
                    "pathId": 6454054798045181898
