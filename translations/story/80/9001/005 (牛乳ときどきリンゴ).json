            "pathId": 6998962284261289666,
            "blockIdx": 6
        },
        {
            "jpName": "駿川たづな",
            "enName": "Tazuna Hayakawa",
            "jpText": "まあ、トレーナーさん……。",
            "enText": "Oh, Trainer...",
            "nextBlock": 8,
            "origClipLength": 55,
            "voiceIdx": 4,
            "voiceSheetId": "059001001",
            "animData": [
                {
                    "origLen": 85,
                    "pathId": -5051623778353090837
