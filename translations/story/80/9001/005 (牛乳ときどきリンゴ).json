            "blockIdx": 14
        },
        {
            "jpName": "駿川たづな",
            "enName": "Tazuna Hayakawa",
            "jpText": "……今の、トレーナーさんですか？",
            "enText": "...Was that you?",
            "nextBlock": 16,
            "origClipLength": 60,
            "voiceIdx": 54,
            "voiceSheetId": "059001001",
            "choices": [
                {
                    "jpText": "たづなさんでもあったような……",
                    "enText": "I don't think it was <i>only</i> me...",
