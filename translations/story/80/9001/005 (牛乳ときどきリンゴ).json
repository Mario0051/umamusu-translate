            "pathId": -951313364041027877,
            "blockIdx": 9
        },
        {
            "jpName": "駿川たづな",
            "enName": "Tazuna Hayakawa",
            "jpText": "はぁっ……お待たせしました～！",
            "enText": "<i>Huff, huff...</i> I'm back!",
            "nextBlock": 11,
            "origClipLength": 95,
            "voiceIdx": 58,
            "voiceSheetId": "059001001",
            "animData": [
