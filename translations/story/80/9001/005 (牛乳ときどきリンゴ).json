            "pathId": -5135196714215813674,
            "blockIdx": 13
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（……ぐぅ）",
            "enText": "<i>Grumble...</i>",
            "nextBlock": 15,
            "origClipLength": 48,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
