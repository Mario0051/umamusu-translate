            "blockIdx": 4
        },
        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "わぁー！　ありがとうございます！",
            "enText": "Yay! Thank you so much!",
            "nextBlock": 6,
            "origClipLength": 65,
            "voiceIdx": 141,
            "voiceSheetId": "051068001",
            "animData": [
                {
                    "origLen": 17,
