            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "まずは揺るぎない自信を見習おう",
                    "enText": "You should try copying her unwavering\nconfidence.",
                    "nextBlock": 8
                },
                {
                    "jpText": "まずはあのバイタリティを見習おう",
                    "enText": "You should try copying her boundless energy.",
                    "nextBlock": 16
