    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "ある日、トレーニングの様子を見ていると――",
            "enText": "One day, as I watch the other racers<n>train...",
            "nextBlock": 2,
            "origClipLength": 58,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": 5266416852811132730,
            "blockIdx": 1
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "はぁ……。",
            "enText": "<i>Sigh...</i>",
            "nextBlock": 3,
            "origClipLength": 74,
            "voiceIdx": 60,
            "voiceSheetId": "051058001",
            "choices": [
                {
                    "jpText": "走らないのか？",
                    "enText": "Aren't you going to run?",
                    "nextBlock": 3
                },
                {
                    "jpText": "走らないの？",
                    "enText": "Aren't you going to run?",
                    "nextBlock": 3
