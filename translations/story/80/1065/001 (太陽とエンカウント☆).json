            "pathId": 5686145296715362350,
            "blockIdx": 5
        },
        {
            "jpName": "ダイタクヘリオス",
            "enName": "Daitaku Helios",
            "jpText": "バイブスぶちあげでぇー\r\nヒアウィーゴー！！！！",
            "enText": "This karaoke sesh's gonna be FIRE! Let's<n>goooo!",
            "nextBlock": 7,
            "origClipLength": 76,
            "voiceIdx": 6,
            "voiceSheetId": "051065001",
            "animData": [
                {
                    "origLen": 87,
                    "pathId": -481988101639256636
