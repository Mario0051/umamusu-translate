            "origClipLength": 54,
            "voiceIdx": 35,
            "voiceSheetId": "051006001",
            "choices": [
                {
                    "jpText": "どうしたんだ？",
                    "enText": "Something wrong?",
                    "nextBlock": 4
                },
                {
                    "jpText": "どうしたの？",
                    "enText": "Something wrong?",
                    "nextBlock": 4
