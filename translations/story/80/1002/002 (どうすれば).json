    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "ある日、息抜きに\r\n公園へやって来たのだが……。",
            "enText": "One day, while I'm taking a breather at<n>the park...",
            "nextBlock": 2,
            "origClipLength": 105,
            "voiceIdx": -1,
