            "pathId": -6447802854565302476,
            "blockIdx": 9
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "ありがとうございます。\r\nさっそく併走、誘ってきます。",
            "enText": "Thank you so much for the advice. I'll try<n>asking\nher to run with me right away.",
            "nextBlock": -1,
            "origClipLength": 110,
            "voiceIdx": 12,
            "voiceSheetId": "051002001",
            "animData": [
                {
                    "origLen": 102,
                    "pathId": 9105829589654268389
