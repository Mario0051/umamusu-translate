            "blockIdx": 2
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "――あら、ごきげんよう。\r\n少しお時間よろしいでしょうか？",
            "enText": "Ah, greetings. May I have some of your<n>time?",
            "nextBlock": 4,
            "origClipLength": 82,
            "voiceIdx": 61,
            "voiceSheetId": "051013001",
            "choices": [
                {
                    "jpText": "大丈夫だよ",
                    "enText": "Sure.",
                    "nextBlock": 4
                }
            ],
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -116502812636701547
                },
                {
                    "origLen": 93,
                    "pathId": 5264649811499218779
