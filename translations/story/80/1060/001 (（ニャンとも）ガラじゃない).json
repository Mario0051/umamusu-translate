            "blockIdx": 3
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "ひゃわぁぁぁ！？\r\nちょ、アンタ学園の……！",
            "enText": "Gyaaah! Y-you're one of the academy's<n>trainers!",
            "nextBlock": 5,
            "origClipLength": 59,
            "voiceIdx": 25,
            "voiceSheetId": "051060001",
            "choices": [
                {
                    "jpText": "ごめん……",
                    "enText": "Sorry...",
                    "nextBlock": 5
                },
                {
                    "jpText": "ごめんね……",
