            "pathId": 1474662092881342365,
            "blockIdx": 4
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "ここだぁ！",
            "enText": "Now!",
            "nextBlock": 6,
            "origClipLength": 48,
            "voiceIdx": 63,
            "voiceSheetId": "051020001",
            "animData": [
