            "blockIdx": 1
        },
        {
            "jpName": "桐生院トレーナー",
            "enName": "Trainer Kiryuin",
            "jpText": "<username>トレーナー！\r\nお疲れ様です。今から帰られるんですか？",
            "enText": "Trainer <username>!<n>\nNice to see you! Are you heading home<n>now?",
            "nextBlock": 3,
            "origClipLength": 69,
            "voiceIdx": 127,
            "voiceSheetId": "059004001",
            "choices": [
                {
                    "jpText": "はい",
                    "enText": "Yep!",
                    "nextBlock": 3
