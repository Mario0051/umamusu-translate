            "animData": [
                {
                    "origLen": 20,
                    "pathId": -5183428847353980337
                },
                {
                    "origLen": 109,
                    "pathId": 1379651067972914435
                },
                {
                    "origLen": 60,
                    "pathId": 8575895784510064382
                }
