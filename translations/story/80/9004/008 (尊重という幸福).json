            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 30,
                    "pathId": 8302507613929459954
                },
                {
                    "origLen": 109,
                    "pathId": 6262792479001254863
                },
                {
                    "origLen": 46,
                    "pathId": 2931590677223710101
                }
