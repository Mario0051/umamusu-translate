            "pathId": -1129019452098754859,
            "blockIdx": 2
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "あら～、\r\nこんにちは～。",
            "enText": "Oh my! Hello.",
            "nextBlock": 4,
            "origClipLength": 61,
            "voiceIdx": 42,
            "voiceSheetId": "051011001",
            "animData": [
                {
                    "origLen": 72,
                    "pathId": 3633728818947571903
