            "origClipLength": 93,
            "voiceIdx": 59,
            "voiceSheetId": "051001001",
            "choices": [
                {
                    "jpText": "ちょっとぎこちなかったかも？",
                    "enText": "It looked a little awkward.",
                    "nextBlock": 4
