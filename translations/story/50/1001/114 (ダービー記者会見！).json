            "origClipLength": 68,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 54,
                    "pathId": -199707652177858699
                },
                {
                    "origLen": 79,
                    "pathId": 2294889740658314602
