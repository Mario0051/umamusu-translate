            "pathId": 3781586797645958108,
            "blockIdx": 4
        },
        {
            "jpName": "スペシャルウィーク",
            "enName": "Special Week",
            "jpText": "あっ、そうです！　遊園地とか！",
            "enText": "Oooh, yeah! An amusement park!",
            "nextBlock": 6,
            "origClipLength": 64,
            "voiceIdx": 48,
            "voiceSheetId": "051001001",
            "animData": [
                {
                    "origLen": 75,
                    "pathId": -3734366833622736771
                },
                {
                    "origLen": 37,
                    "pathId": -3712529388251071804
                }
