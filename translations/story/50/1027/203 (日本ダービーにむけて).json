                }
            ],
            "pathId": 71105832261144290,
            "blockIdx": 13
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "必ず逃げ切るの！",
            "enText": "I swear I'll take the lead!",
            "nextBlock": 15,
            "origClipLength": 55,
            "voiceIdx": 10,
            "voiceSheetId": "051031001",
