            "animData": [
                {
                    "origLen": 69,
                    "pathId": 6861736347941879435
                },
                {
                    "origLen": 33,
                    "pathId": 4319000594606318848
                }
            ],
            "pathId": 196128941519794301,
            "blockIdx": 19
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "え……？",
            "enText": "What?",
            "nextBlock": 21,
            "origClipLength": 50,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "1番強いウマ娘になるんだ",
                    "enText": "You can still become the strongest of all racers.",
                    "nextBlock": 21
                },
                {
                    "jpText": "1番強いウマ娘になるんだよ",
                    "enText": "You can still become the strongest of all racers.",
                    "nextBlock": 21
                }
            ],
            "animData": [
                {
                    "origLen": 61,
                    "pathId": -4497367038199019698
                },
                {
                    "origLen": 25,
                    "pathId": 8254117039708484532
                }
