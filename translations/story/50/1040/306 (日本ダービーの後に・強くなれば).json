            "pathId": 825158180209281784,
            "blockIdx": 24
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "え……。",
            "enText": "Uh...",
            "nextBlock": 26,
            "origClipLength": 50,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "君の強さを周りにわかってもらいたい！",
                    "enText": "I want people to recognize your strength!",
                    "nextBlock": 26
                },
                {
                    "jpText": "貴方の強さを周りにわかってもらいたい！",
                    "enText": "I want people to recognize your strength!",
                    "nextBlock": 26
                }
