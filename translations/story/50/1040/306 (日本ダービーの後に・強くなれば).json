            "pathId": 5082802415562874453,
            "blockIdx": 13
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "……。",
            "enText": "...",
            "nextBlock": 15,
            "origClipLength": 48,
            "voiceIdx": 110,
            "voiceSheetId": "051040001",
            "choices": [
                {
                    "jpText": "いい走り、見せたな！",
                    "enText": "You ran great out there!",
                    "nextBlock": 15
                },
                {
                    "jpText": "いい走り、見せたね！",
                    "enText": "You ran great out there!",
                    "nextBlock": 15
                }
            ],
            "animData": [
                {
                    "origLen": 103,
                    "pathId": -8141897601361625449
                },
                {
                    "origLen": 106,
                    "pathId": -8461028927490945191
                },
                {
                    "origLen": 96,
                    "pathId": 18004203924704005
                }
