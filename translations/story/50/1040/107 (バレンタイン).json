            "pathId": -2390313855387391956,
            "blockIdx": 27
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "はい、ハッピーバレンタイン。",
            "enText": "Happy Valentine's Day.",
            "nextBlock": 29,
            "origClipLength": 63,
            "voiceIdx": 134,
            "voiceSheetId": "051040001",
            "animData": [
                {
                    "origLen": 105,
                    "pathId": -4566544960015374697
                },
                {
                    "origLen": 208,
                    "pathId": 4539752807384500899
                },
                {
                    "origLen": 134,
                    "pathId": -979692276064616281
                }
            ],
            "pathId": -2970761106069908816,
            "blockIdx": 28
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "一応、世話になってる礼ってことで。",
            "enText": "For all the things you do for me.",
            "nextBlock": 30,
            "origClipLength": 67,
            "voiceIdx": 92,
            "voiceSheetId": "051040001",
            "choices": [
                {
                    "jpText": "あ、ありがとう……！",
                    "enText": "Th-thanks!",
                    "nextBlock": 30
                }
            ],
            "animData": [
                {
                    "origLen": 10,
                    "pathId": -6687784557823874399
                },
                {
                    "origLen": 151,
                    "pathId": -3854475033061630536
                },
                {
                    "origLen": 127,
                    "pathId": -976162024758912997
