                {
                    "origLen": 102,
                    "pathId": 1955045653019165471
                }
            ],
            "pathId": -6650875555996383953,
            "blockIdx": 5
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "……っ。",
            "enText": "...",
            "nextBlock": 7,
            "origClipLength": 50,
            "voiceIdx": 76,
            "voiceSheetId": "051040001",
            "animData": [
                {
                    "origLen": 61,
                    "pathId": -1126402951505484465
