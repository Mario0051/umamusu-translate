                {
                    "origLen": 108,
                    "pathId": 2138366722276680788
                },
                {
                    "origLen": 175,
                    "pathId": 6677280911815094870
                }
            ],
            "pathId": -336866109408814632,
            "blockIdx": 16
        }
    ]
