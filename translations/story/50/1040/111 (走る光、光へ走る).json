            "pathId": 3849036235735216523,
            "blockIdx": 17
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "……！",
            "enText": "...!",
            "nextBlock": 19,
            "origClipLength": 48,
