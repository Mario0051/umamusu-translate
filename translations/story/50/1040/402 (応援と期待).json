    "text": [
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "友人であるトーセンジョーダンから、\nゴールドシチーは自分の評判を聞く。",
            "enText": "Gold City becomes aware of her<n>reputation\nthrough her friend Tosen<n>Jordan.",
            "nextBlock": 2,
            "origClipLength": 90,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "coloredText": [
                {
                    "jpText": "トーセンジョーダン",
                    "enText": ""
                }
            ],
            "animData": [
                {
                    "origLen": 172,
                    "pathId": 2237428218148242549
                },
