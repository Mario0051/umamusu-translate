            "pathId": 7086961643560287987,
            "blockIdx": 19
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "顔とか見てる暇ないくらいの走りで、\nビビらせてやるんだ……！",
            "enText": "I'll surprise them by running so fast they<n>won't\nhave the chance to see my face!",
            "nextBlock": 21,
            "origClipLength": 84,
            "voiceIdx": 10,
            "voiceSheetId": "051040001",
            "choices": [
                {
                    "jpText": "君らしく走ってきて",
                    "enText": "Go and run like yourself.",
                    "nextBlock": 21
                },
                {
                    "jpText": "貴方らしく走ってきて",
                    "enText": "Go and run like yourself.",
                    "nextBlock": 21
