            "origClipLength": 67,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "珍しく本気なんだな",
                    "enText": "Rare to see you so serious about this.",
                    "nextBlock": 3
                },
                {
                    "jpText": "珍しく本気なんだね",
                    "enText": "Rare to see you so serious about this.",
                    "nextBlock": 3
                }
