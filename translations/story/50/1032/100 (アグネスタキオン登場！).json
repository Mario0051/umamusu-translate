            "nextBlock": 41,
            "origClipLength": 52,
            "voiceIdx": 53,
            "voiceSheetId": "051032001",
            "choices": [
                {
                    "jpText": "君がすごく速いことが改めてわかった！",
                    "enText": "I was reminded of how fast you are!",
                    "nextBlock": 41
