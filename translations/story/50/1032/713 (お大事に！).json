            "nextBlock": 2,
            "origClipLength": 71,
            "voiceIdx": 21,
            "voiceSheetId": "051032001",
            "choices": [
                {
                    "jpText": "そんなことはない",
                    "enText": "I don't think so.",
                    "nextBlock": 2
                }
