            "pathId": -5631347501597937001,
            "blockIdx": 15
        },
        {
            "jpName": "アグネスタキオン",
            "enName": "Agnes Tachyon",
            "jpText": "そうこなくっちゃね。",
            "enText": "I wouldn't expect anything less!",
            "nextBlock": 17,
            "origClipLength": 51,
            "voiceIdx": 5,
            "voiceSheetId": "051032001",
            "animData": [
                {
                    "origLen": 31,
