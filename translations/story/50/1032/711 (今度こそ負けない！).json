            "origClipLength": 67,
            "voiceIdx": 48,
            "voiceSheetId": "051032001",
            "choices": [
                {
                    "jpText": "ゆっくり考えよう",
                    "enText": "Take your time and think it through.",
                    "nextBlock": 4
                },
                {
                    "jpText": "悩むより前に進もう！",
                    "enText": "Let's move forward without worrying!",
                    "nextBlock": 8
                }
