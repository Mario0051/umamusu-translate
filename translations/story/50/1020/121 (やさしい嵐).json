            "pathId": -2207095683812906479,
            "blockIdx": 13
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "じ、じいちゃん！\n待って――わぶっ。",
            "enText": "G-Gramps! Wait a—erf.",
            "nextBlock": 15,
            "origClipLength": 68,
            "voiceIdx": 25,
            "voiceSheetId": "051020001",
            "animData": [
                {
                    "origLen": 12,
                    "pathId": 3455788548225265696
                },
                {
                    "origLen": 12,
                    "pathId": -3466204401879806725
                }
