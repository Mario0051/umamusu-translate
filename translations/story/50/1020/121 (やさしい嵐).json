            "pathId": 5023491686975789938,
            "blockIdx": 6
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "……っ！？\nは、え……！？",
            "enText": "Huh?! Who's—",
            "nextBlock": 8,
            "origClipLength": 61,
            "voiceIdx": 26,
            "voiceSheetId": "051020001",
            "animData": [
                {
                    "origLen": 72,
                    "pathId": 2550205450469613067
