                {
                    "origLen": 53,
                    "pathId": 471274589382378012
                }
            ],
            "pathId": -8083731450196302192,
            "blockIdx": 5
        },
        {
            "jpName": "？？？",
            "enName": "???",
            "jpText": "スカイ！！！！",
            "enText": "SKY!",
            "nextBlock": 7,
            "origClipLength": 54,
            "voiceIdx": -1,
            "voiceSheetId": "",
