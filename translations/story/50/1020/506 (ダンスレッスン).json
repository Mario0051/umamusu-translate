                {
                    "origLen": 102,
                    "pathId": 5972387121405386699
                },
                {
                    "origLen": 102,
                    "pathId": -5030401600477719894
                }
            ],
            "pathId": -5398391046827796568,
            "blockIdx": 24
        }
    ]
