        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "釣りって、お好きだったりします？",
            "enText": "Do you like fishing?",
            "nextBlock": 4,
            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "いきなり何の話？",
