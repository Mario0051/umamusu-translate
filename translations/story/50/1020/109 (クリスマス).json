            "nextBlock": 33,
            "origClipLength": 52,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 89,
                    "pathId": -7553982141132508927
                },
                {
                    "origLen": 63,
                    "pathId": 2798591092370449984
                }
