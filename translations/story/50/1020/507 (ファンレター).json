            "animData": [
                {
                    "origLen": 87,
                    "pathId": 4168470467283033661
                },
                {
                    "origLen": 54,
                    "pathId": -5593629946042690283
                }
            ],
            "pathId": -4983632140447614055,
            "blockIdx": 5
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "『予想できないレースで楽しませてくれる\nセイウンスカイさんが大好きです！』",
            "enText": "\"Dear Seiun Sky, I love you and your<n>\nunpredictable races!\"",
            "nextBlock": 7,
            "origClipLength": 93,
            "voiceIdx": 79,
            "voiceSheetId": "051020001",
            "animData": [
                {
                    "origLen": 136,
                    "pathId": 8262964679458731908
                },
                {
                    "origLen": 129,
                    "pathId": -3330590451009538387
                }
            ],
            "pathId": -5266860399412314294,
            "blockIdx": 6
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "『先日、お昼寝しているのを見かけました。\r\n声をかけられても全然起きませんでしたね。』",
            "enText": "\"I saw you taking a nap the other day,<n>but when I\ncalled out to you, you kept<n>sleeping.\"",
            "nextBlock": 8,
            "origClipLength": 101,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 112,
                    "pathId": -3091956027086218320
                },
                {
                    "origLen": 112,
                    "pathId": -4511517336439931286
                }
            ],
            "pathId": 1615273790945294760,
            "blockIdx": 7
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "『よければ、この時計を使ってください。\nバッチリ起きられると思います！』",
            "enText": "\"Feel free to use this clock so you can<n>wake up\nright on time!\"",
            "nextBlock": 9,
            "origClipLength": 91,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 102,
                    "pathId": -6299388853259987717
                },
                {
                    "origLen": 102,
                    "pathId": -8551726392297794634
                }
            ],
            "pathId": 3558642313037598128,
            "blockIdx": 8
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "……だってさ。",
            "enText": "...Hm.",
            "nextBlock": 10,
            "origClipLength": 54,
            "voiceIdx": 31,
            "voiceSheetId": "051020001",
            "choices": [
                {
                    "jpText": "よかったな",
                    "enText": "That's so nice.",
                    "nextBlock": 10
                },
                {
                    "jpText": "よかったね",
                    "enText": "That's so nice.",
                    "nextBlock": 10
