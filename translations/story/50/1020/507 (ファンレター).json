            "origClipLength": 88,
            "voiceIdx": 79,
            "voiceSheetId": "051020001",
            "choices": [
                {
                    "jpText": "そんな家訓あるのか？",
                    "enText": "Your family has a motto?",
                    "nextBlock": 18
                },
                {
                    "jpText": "そんな家訓あるの？",
                    "enText": "Your family has a motto?",
                    "nextBlock": 18
