            "pathId": 5580873247845614576,
            "blockIdx": 5
        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "……スカイさ～ん！！　出てきてくださ～い！\n私の運命がかかっているんです～！",
            "enText": "Sky! Pleeease come out! My very fate is<n>resting on\nthis!",
            "nextBlock": 7,
            "origClipLength": 94,
            "voiceIdx": 23,
            "voiceSheetId": "051056001",
            "animData": [
                {
                    "origLen": 144,
                    "pathId": -6202639467845284990
                },
                {
                    "origLen": 157,
                    "pathId": 8353870560281586439
