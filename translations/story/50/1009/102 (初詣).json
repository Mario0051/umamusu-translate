            "pathId": -6212415565459146162,
            "blockIdx": 12
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "ぐっ……。",
            "enText": "Ngh...",
            "nextBlock": 14,
            "origClipLength": 48,
            "voiceIdx": 18,
            "voiceSheetId": "051009001",
            "animData": [
                {
                    "origLen": 59,
                    "pathId": -411018663256785924
