            "pathId": 4372494210318117182,
            "blockIdx": 9
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "……帰りましょ、トレーナー。",
            "enText": "...Let's go, Trainer.",
            "nextBlock": 11,
            "origClipLength": 54,
            "voiceIdx": 57,
            "voiceSheetId": "051009001",
            "animData": [
                {
                    "origLen": 65,
                    "pathId": -76696346796428874
