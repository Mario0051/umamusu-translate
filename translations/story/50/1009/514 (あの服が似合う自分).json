            "blockIdx": 2
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Umamusume A",
            "jpText": "――――ごめんなさい！",
            "enText": "I'm sorry!",
            "nextBlock": 4,
            "origClipLength": 52,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 63,
                    "pathId": 6707979379153207274
