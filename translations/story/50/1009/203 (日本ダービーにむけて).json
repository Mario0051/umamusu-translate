            "origClipLength": 58,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": -862646495786141788,
            "blockIdx": 10
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "…………。\r\n……あ。",
            "enText": "...Oh, right.",
            "nextBlock": 12,
            "origClipLength": 52,
            "voiceIdx": 78,
            "voiceSheetId": "051009001",
            "animData": [
                {
                    "origLen": 98,
                    "pathId": -9058899352912063220
