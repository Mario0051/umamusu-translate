            "pathId": -1089791672616114808,
            "blockIdx": 9
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "……1番人気、か。",
            "enText": "Top of the polls, huh...",
            "nextBlock": 11,
            "origClipLength": 50,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "プレッシャー？",
                    "enText": "Feeling the pressure?",
                    "nextBlock": 11
                }
