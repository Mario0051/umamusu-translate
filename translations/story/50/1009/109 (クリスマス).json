            "pathId": 3805179325370111653,
            "blockIdx": 5
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "……あ、流れた！　ほら！！",
            "enText": "Ah! Look! It's starting!",
            "nextBlock": 7,
            "origClipLength": 53,
            "voiceIdx": 31,
            "voiceSheetId": "051009001",
            "animData": [
                {
                    "origLen": 54,
