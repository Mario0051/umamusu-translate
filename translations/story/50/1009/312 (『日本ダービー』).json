            "pathId": 8502266803279486773,
            "blockIdx": 19
        },
        {
            "jpName": "観客",
            "enName": "Crowd",
            "jpText": "（ワァーーーーーーー！！）",
            "enText": "<i>Wooooo!</i>",
            "nextBlock": 21,
            "origClipLength": 53,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
