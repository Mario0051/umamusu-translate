            "pathId": -782993143064178088,
            "blockIdx": 13
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "んなっ！？\r\nアンタ、マジで言ってるの！？",
            "enText": "Wha—?! Are you being serious right<n>now?!",
            "nextBlock": 15,
            "origClipLength": 58,
            "voiceIdx": 16,
            "voiceSheetId": "051009001",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 8776030544790618443
                },
                {
                    "origLen": 69,
                    "pathId": 9008972164419011681
                },
                {
                    "origLen": 50,
                    "pathId": -1331902919633768163
                }
