            "nextBlock": 19,
            "origClipLength": 72,
            "voiceIdx": 35,
            "voiceSheetId": "051009001",
            "choices": [
                {
                    "jpText": "なんで俺の買い物に……？",
                    "enText": "...Why are we shopping for me?",
                    "nextBlock": 19
                },
                {
                    "jpText": "なんで私の買い物に……？",
                    "enText": "...Why are we shopping for me?",
                    "nextBlock": 19
