        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "なんだ、トレーナーか……。\r\n驚かせないでちょうだい。",
            "enText": "Oh, it's only you... Don't scare me like<n>that.",
            "nextBlock": 5,
            "origClipLength": 62,
            "voiceIdx": 15,
            "voiceSheetId": "051009001",
            "choices": [
                {
                    "jpText": "ごめん、集中してたんだな",
                    "enText": "Sorry. I didn't mean to interrupt.",
                    "nextBlock": 5
                },
                {
                    "jpText": "ごめん、集中してたんだね",
                    "enText": "Sorry. I didn't mean to interrupt.",
                    "nextBlock": 5
