            "pathId": 3122907991421231612,
            "blockIdx": 11
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "っ！　うん！　行きましょ！",
            "enText": "Yeah! Right! Let's go!",
            "nextBlock": 13,
            "origClipLength": 53,
            "voiceIdx": 26,
            "voiceSheetId": "051009001",
            "animData": [
                {
                    "origLen": 64,
                    "pathId": 4990473930528870137
                },
                {
                    "origLen": 46,
                    "pathId": -7234198203209542186
                }
