                {
                    "origLen": 78,
                    "pathId": 7911842676655488692
                }
            ],
            "pathId": 1405213428853529567,
            "blockIdx": 1
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "……む？",
            "enText": "...Hm?",
            "nextBlock": 3,
            "origClipLength": 47,
            "voiceIdx": 26,
            "voiceSheetId": "051017001",
            "animData": [
                {
                    "origLen": 58,
