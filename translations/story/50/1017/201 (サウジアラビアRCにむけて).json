            "pathId": -2028278070329563184,
            "blockIdx": 23
        },
        {
            "jpName": "生徒会庶務",
            "enName": "Student Council General Manager",
            "jpText": "は、はいっ……！",
            "enText": "O-of course!",
            "nextBlock": 25,
            "origClipLength": 50,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 61,
                    "pathId": -4674418810453314725
                },
                {
                    "origLen": 61,
                    "pathId": 3921124715678744232
                },
