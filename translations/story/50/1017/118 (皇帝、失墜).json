        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "……ああ！",
            "enText": "...Ah!",
            "nextBlock": 20,
            "origClipLength": 48,
            "voiceIdx": 25,
            "voiceSheetId": "051017001",
            "animData": [
                {
                    "origLen": 59,
