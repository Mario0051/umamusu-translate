            "blockIdx": 7
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "トレーナー君……！",
            "enText": "Trainer...",
            "nextBlock": 9,
            "origClipLength": 50,
            "voiceIdx": 67,
            "voiceSheetId": "051017001",
