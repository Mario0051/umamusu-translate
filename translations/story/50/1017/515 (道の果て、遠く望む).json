    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "シンボリルドルフを探して\r\n夕方の学園内を歩いていると――",
            "enText": "One evening, I'm wandering the campus<n>grounds\nin search of Symboli Rudolf.",
            "nextBlock": 2,
            "origClipLength": 63,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": -7552067729726439312,
            "blockIdx": 1
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "――――",
            "enText": "...",
            "nextBlock": 3,
            "origClipLength": 47,
            "voiceIdx": 79,
            "voiceSheetId": "051017001",
            "animData": [
                {
                    "origLen": 46,
