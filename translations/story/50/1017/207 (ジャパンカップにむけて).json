            "pathId": -6003965887832538547,
            "blockIdx": 2
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Umamusume A",
            "jpText": "はいっ、失礼しました……！",
            "enText": "Right! Um... I'll be going, then!",
            "nextBlock": 4,
            "origClipLength": 53,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "あの子、今日一緒に走る子だな",
                    "enText": "Is she racing with you today?",
                    "nextBlock": 4
                },
                {
                    "jpText": "あの子、今日一緒に走る子だね",
                    "enText": "Is she racing with you today?",
                    "nextBlock": 4
                }
            ],
            "animData": [
                {
                    "origLen": 64,
                    "pathId": -5964387367038490498
                },
                {
                    "origLen": 89,
