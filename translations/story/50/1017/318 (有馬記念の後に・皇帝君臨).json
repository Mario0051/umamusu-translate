            "origClipLength": 70,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 81,
                    "pathId": 6359957484594043052
                },
                {
                    "origLen": 44,
                    "pathId": -8068194575919412132
                }
