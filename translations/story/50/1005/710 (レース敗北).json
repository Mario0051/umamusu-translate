            "blockIdx": 11
        },
        {
            "jpName": "フジキセキ",
            "enName": "Fuji Kiseki",
            "jpText": "ありがとう……！\r\n君は心強いパートナーだよ！",
            "enText": "And thank <i>you</i> for believing in me!",
            "nextBlock": 13,
            "origClipLength": 75,
            "voiceIdx": 13,
            "voiceSheetId": "051005001",
            "animData": [
