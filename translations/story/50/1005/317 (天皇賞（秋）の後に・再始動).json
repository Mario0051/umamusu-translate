            "blockIdx": 12
        },
        {
            "jpName": "フジキセキ",
            "enName": "Fuji Kiseki",
            "jpText": "掴めたよ、トレーナーさん！",
            "enText": "Trainer, I finally understand!",
            "nextBlock": 14,
            "origClipLength": 61,
            "voiceIdx": 56,
            "voiceSheetId": "051005001",
            "animData": [
