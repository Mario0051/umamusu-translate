            "blockIdx": 1
        },
        {
            "jpName": "フジキセキ",
            "enName": "Fuji Kiseki",
            "jpText": "くっ……！\r\n……平気だよ……とは言えないかな……っ。",
            "enText": "Guh! I'm fine... or so I'd like to say, but<n>I'd be\nlying... Argh.",
            "nextBlock": 3,
            "origClipLength": 80,
            "voiceIdx": 109,
            "voiceSheetId": "051005001",
