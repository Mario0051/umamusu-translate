    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "年が明け、\r\nフジキセキとともに初詣へ来ていた。",
            "enText": "To celebrate New Year's, Fuji Kiseki and<n>I go on\nour first shrine visit of the year<n>together.",
            "nextBlock": 2,
            "origClipLength": 76,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 87,
                    "pathId": -4129788833853341741
