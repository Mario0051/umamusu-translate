            "blockIdx": 5
        },
        {
            "jpName": "フジキセキ",
            "enName": "Fuji Kiseki",
            "jpText": "！",
            "enText": "...!",
            "nextBlock": 7,
            "origClipLength": 46,
            "voiceIdx": 30,
