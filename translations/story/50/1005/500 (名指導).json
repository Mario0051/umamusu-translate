    "text": [
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "ある日、フジキセキが廊下を\r\n歩いている時のこと……。",
            "enText": "One day, while Fuji Kiseki is walking<n>down the\nhallway...",
            "nextBlock": 2,
            "origClipLength": 80,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 114,
                    "pathId": -6359666240509097976
