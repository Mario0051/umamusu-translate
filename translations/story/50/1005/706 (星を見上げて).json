    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "フジキセキと出かけ、\r\n軽い用事を済ませた帰り道……。",
            "enText": "After wrapping up some errands with<n>Fuji Kiseki,\nwe start heading home.",
            "nextBlock": 2,
            "origClipLength": 80,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 91,
                    "pathId": -9098429853791748989
