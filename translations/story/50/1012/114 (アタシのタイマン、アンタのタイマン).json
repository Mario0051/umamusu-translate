    "text": [
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "どありゃあああああっ！！",
            "enText": "Raaaaah!",
            "nextBlock": 2,
            "origClipLength": 81,
