            "blockIdx": 31
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "あ、悪い……。",
            "enText": "Oh, my bad...",
            "nextBlock": 33,
            "origClipLength": 54,
            "voiceIdx": 24,
            "voiceSheetId": "051012001",
