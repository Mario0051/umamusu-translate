            "pathId": 3191146204265541701,
            "blockIdx": 30
        },
        {
            "jpName": "図書室の先生",
            "enName": "Library Assistant",
            "jpText": "図書室ではお静かにお願いします。",
            "enText": "Please keep quiet in the library.",
            "nextBlock": 32,
            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
