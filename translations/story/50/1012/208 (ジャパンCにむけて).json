            "pathId": 3211370066912751561,
            "blockIdx": 18
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "……！\r\nへへっ、ああ、その通りだ！",
            "enText": "Hehe. Yeah, exactly!",
            "nextBlock": 20,
            "origClipLength": 68,
            "voiceIdx": 0,
            "voiceSheetId": "051012001",
            "animData": [
