    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "最近不調のヒシアマゾンに、\r\nお参りしていこうと誘われた――",
            "enText": "Hishi Amazon has been feeling off her<n>game\nlately, so she asks me to go to a<n>shrine with her.",
            "nextBlock": 2,
            "origClipLength": 84,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "神頼みなんて意外だな",
                    "enText": "I didn't take you as the type to pray for favors.",
                    "nextBlock": 2
                },
                {
                    "jpText": "神頼みなんて意外だね",
                    "enText": "I didn't take you as the type to pray for favors.",
                    "nextBlock": 2
                }
            ],
