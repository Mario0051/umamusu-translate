            "pathId": -5931549367141356383,
            "blockIdx": 20
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……っ！",
            "enText": "...!",
            "nextBlock": 22,
            "origClipLength": 50,
            "voiceIdx": 17,
            "voiceSheetId": "051016001",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 7731114733549917477
                },
