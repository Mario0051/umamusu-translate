            "pathId": 4988322992473052035,
            "blockIdx": 3
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……はぁあ――！",
            "enText": "Raaah!",
            "nextBlock": 5,
            "origClipLength": 56,
            "voiceIdx": 125,
            "voiceSheetId": "051016001",
            "animData": [
                {
                    "origLen": 58,
                    "pathId": -3022274501307519614
