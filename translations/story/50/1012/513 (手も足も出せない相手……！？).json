            "nextBlock": 17,
            "origClipLength": 61,
            "voiceIdx": 104,
            "voiceSheetId": "051012001",
            "choices": [
                {
                    "jpText": "で、出た！！",
                    "enText": "I-it really jumped out!",
                    "nextBlock": 17
                }
