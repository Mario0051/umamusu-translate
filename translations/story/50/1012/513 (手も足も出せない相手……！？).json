        },
        {
            "jpName": "幽霊の犠牲者",
            "enName": "Movie Victim",
            "jpText": "『ぐああぁぁっ……！！』",
            "enText": "Arrrrgghh!",
            "nextBlock": 18,
            "origClipLength": 60,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 71,
