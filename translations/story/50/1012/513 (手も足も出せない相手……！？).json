        {
            "jpName": "映画の幽霊",
            "enName": "Movie Ghost",
            "jpText": "『――ウオオオオォォォォン！！』",
            "enText": "RAAAGH!",
            "nextBlock": 16,
            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 16,
