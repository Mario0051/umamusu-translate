    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "ヒシアマゾンはトレーニングコースで、\r\n調整した勝負服の着心地を確かめていた――",
            "enText": "At the training track, Hishi Amazon tries<n>out her\nsignature racewear to see how<n>well it fits.",
            "nextBlock": 2,
            "origClipLength": 97,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": -6205566511196383204,
            "blockIdx": 1
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "――うっし！！",
            "enText": "Nice!",
            "nextBlock": 3,
            "origClipLength": 54,
            "voiceIdx": 5,
            "voiceSheetId": "051012001",
            "animData": [
                {
                    "origLen": 65,
