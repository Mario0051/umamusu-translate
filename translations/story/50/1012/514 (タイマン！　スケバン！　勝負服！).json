            "blockIdx": 25
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "それじゃあ――ビシっと！\r\nこんな感じでどうだい？",
            "enText": "Ahoy there! How's this?",
            "nextBlock": 27,
            "origClipLength": 77,
            "voiceIdx": 87,
            "voiceSheetId": "051012001",
            "choices": [
                {
                    "jpText": "カッコいい！",
                    "enText": "Very cool!",
                    "nextBlock": 27
