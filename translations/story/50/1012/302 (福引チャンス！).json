            "blockIdx": 19
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "――なぁっ！？\r\n本当だ、2人用じゃないか！",
            "enText": "Hang on, you're right!",
            "nextBlock": 21,
            "origClipLength": 73,
            "voiceIdx": 31,
            "voiceSheetId": "051012001",
            "animData": [
