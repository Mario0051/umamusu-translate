            "pathId": -6225856208816521482,
            "blockIdx": 2
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "ここまでいい香りがしてくる。\r\nへへっ。楽しみだ！",
            "enText": "I can smell the hot spring all the way<n>from here.\nHehe, I can't wait to jump in!",
            "nextBlock": 4,
            "origClipLength": 77,
            "voiceIdx": 0,
            "voiceSheetId": "051012001",
            "animData": [
                {
                    "origLen": 148,
