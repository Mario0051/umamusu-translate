            "pathId": -1565180727098132344,
            "blockIdx": 19
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "なんだか実家を思い出すよ。",
            "enText": "Being here reminds me of home.",
            "nextBlock": 21,
            "origClipLength": 61,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 72,
