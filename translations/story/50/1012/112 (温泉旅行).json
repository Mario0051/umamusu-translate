            "pathId": -4349732165609115427,
            "blockIdx": 22
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "……なぁトレ公。",
            "enText": "Hey, Teach?",
            "nextBlock": 24,
            "origClipLength": 55,
            "voiceIdx": 54,
            "voiceSheetId": "051012001",
            "choices": [
