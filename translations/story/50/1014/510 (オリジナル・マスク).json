            "pathId": -5070030059427395455,
            "blockIdx": 14
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "――そして。",
            "enText": "A bit later...",
            "nextBlock": 16,
            "origClipLength": 52,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
