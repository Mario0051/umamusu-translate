            "pathId": -2656083218137226532,
            "blockIdx": 1
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "『ジャパンカップ』デェェェス！\r\nファンのみんな、オラー【こんにちは】！！",
            "enText": "The Japan Cup! Hola, everyone!",
            "nextBlock": 3,
            "origClipLength": 93,
            "voiceIdx": 1,
