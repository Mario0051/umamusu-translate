            "blockIdx": 3
        },
        {
            "jpName": "観客A",
            "enName": "Fan A",
            "jpText": "ええっ、エルコンドルパサー！？",
            "enText": "Is that El Condor Pasa?!",
            "nextBlock": 5,
            "origClipLength": 64,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 75,
