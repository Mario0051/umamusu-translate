        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "ふっふふ～んっ♪",
            "enText": "Hmhmhm. ♪",
            "nextBlock": 3,
            "origClipLength": 50,
            "voiceIdx": 75,
            "voiceSheetId": "051014001",
            "choices": [
                {
                    "jpText": "いいことでもあったのか？",
                    "enText": "Did something good happen?",
                    "nextBlock": 3
                },
                {
                    "jpText": "いいことでもあったの？",
                    "enText": "Did something good happen?",
                    "nextBlock": 3
                }
            ],
            "animData": [
