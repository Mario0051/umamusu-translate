            "pathId": 2133167745436164609,
            "blockIdx": 2
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "情けないデス……。\r\nまた、エルは羽ばたけませんでした……！",
            "enText": "I'm pathetic... I failed to spread my wings<n>yet\nagain...",
            "nextBlock": 4,
            "origClipLength": 84,
            "voiceIdx": 60,
            "voiceSheetId": "051014001",
            "choices": [
                {
                    "jpText": "1度ゆっくりしてみよう",
                    "enText": "You should take it easy for once.",
                    "nextBlock": 4
                },
                {
                    "jpText": "立て！立つんだエル！",
                    "enText": "Get up, El! Stand up tall!",
                    "nextBlock": 10
                }
