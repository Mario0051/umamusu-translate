            "blockIdx": 11
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "うぅぅぅーーん！\r\nアグアンタテッ！　エルなら出来ますデース！",
            "enText": "Urgh! Aguántate, El! You can do this!",
            "nextBlock": 13,
            "origClipLength": 65,
            "voiceIdx": 10,
            "voiceSheetId": "051014001",
            "animData": [
                {
                    "origLen": 76,
