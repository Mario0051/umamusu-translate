            "blockIdx": 21
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "……ふぅ……。",
            "enText": "<i>Phew...</i>",
            "nextBlock": 23,
            "origClipLength": 54,
            "voiceIdx": 59,
            "voiceSheetId": "051023001",
            "choices": [
                {
                    "jpText": "……やり切ったな",
                    "enText": "You did it!",
                    "nextBlock": 23
