                {
                    "origLen": 60,
                    "pathId": 8256397368439394937
                }
            ],
            "pathId": -5027429818762815025,
            "blockIdx": 5
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "ただ嬉しいと思っただけで……。",
            "enText": "I'm just so happy...",
            "nextBlock": 7,
            "origClipLength": 64,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "嬉しい？",
                    "enText": "Happy?",
                    "nextBlock": 7
