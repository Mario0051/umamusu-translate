            "blockIdx": 15
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "……ほう？",
            "enText": "Oh?",
            "nextBlock": 17,
            "origClipLength": 51,
            "voiceIdx": 7,
            "voiceSheetId": "051023001",
            "animData": [
                {
                    "origLen": 63,
