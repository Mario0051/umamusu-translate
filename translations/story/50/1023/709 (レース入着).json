    "text": [
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "これが、今の私が出来得るレースか。",
            "enText": "So this is the current extent of my<n>abilities...",
            "nextBlock": 2,
            "origClipLength": 67,
            "voiceIdx": 34,
            "voiceSheetId": "051023001",
