            "blockIdx": 3
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "あっ！",
            "enText": "Oh!",
            "nextBlock": 5,
            "origClipLength": 48,
            "voiceIdx": 77,
            "voiceSheetId": "051023001",
            "animData": [
