            "nextBlock": 22,
            "origClipLength": 90,
            "voiceIdx": 5,
            "voiceSheetId": "051023001",
            "choices": [
                {
                    "jpText": "飛ばしすぎには注意",
                    "enText": "Don't push yourself too hard. ",
