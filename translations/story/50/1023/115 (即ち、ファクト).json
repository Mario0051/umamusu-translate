            "nextBlock": 11,
            "origClipLength": 77,
            "voiceIdx": 74,
            "voiceSheetId": "051023001",
            "choices": [
                {
                    "jpText": "でも君には集中してほしいから",
                    "enText": "I need you to stay focused.",
                    "nextBlock": 11
                },
                {
                    "jpText": "でも貴方には集中してほしいから",
                    "enText": "I need you to stay focused.",
                    "nextBlock": 11
