            "pathId": -4951093693356047778,
            "blockIdx": 3
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "そうか？　悪いな。",
            "enText": "Really? Thank you.",
            "nextBlock": 5,
            "origClipLength": 56,
            "voiceIdx": 12,
            "voiceSheetId": "051023001",
            "animData": [
                {
                    "origLen": 67,
