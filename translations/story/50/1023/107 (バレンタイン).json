            "pathId": 1340668133678240140,
            "blockIdx": 24
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "………………。",
            "enText": "...",
            "nextBlock": 26,
            "origClipLength": 54,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "どうしたんだ？",
                    "enText": "Is something wrong?",
                    "nextBlock": 26
                },
                {
                    "jpText": "どうしたの？",
                    "enText": "Is something wrong?",
                    "nextBlock": 26
