            "pathId": -3754215652680103876,
            "blockIdx": 6
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "はっ！？",
            "enText": "Gah!",
            "nextBlock": 8,
            "origClipLength": 50,
            "voiceIdx": 111,
            "voiceSheetId": "051023001",
            "animData": [
                {
                    "origLen": 61,
