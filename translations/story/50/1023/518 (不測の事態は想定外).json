    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "朝、始業時間近くのこと――",
            "enText": "Just as morning classes are about to<n>start...",
            "nextBlock": 2,
            "origClipLength": 61,
            "voiceIdx": -1,
            "voiceSheetId": "",
