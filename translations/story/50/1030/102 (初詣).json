            "origClipLength": 67,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 78,
                    "pathId": 3144130720100565880
                },
                {
                    "origLen": 48,
                    "pathId": 8043008177753759816
                }
