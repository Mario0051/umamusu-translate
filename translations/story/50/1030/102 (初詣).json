            "origClipLength": 57,
            "voiceIdx": 11,
            "voiceSheetId": "051030001",
            "choices": [
                {
                    "jpText": "どうしてだ？",
                    "enText": "Why's that?",
                    "nextBlock": 8
                },
                {
                    "jpText": "どうして？",
