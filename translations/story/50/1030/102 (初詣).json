            "origClipLength": 67,
            "voiceIdx": 69,
            "voiceSheetId": "051030001",
            "animData": [
                {
                    "origLen": 78,
                    "pathId": -2678974892426784742
                },
                {
                    "origLen": 40,
                    "pathId": -8027849834391705752
