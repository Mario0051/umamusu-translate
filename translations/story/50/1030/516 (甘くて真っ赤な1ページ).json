            "pathId": 4833857877372420986,
            "blockIdx": 16
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "別腹……？\r\n食べて、いいの？",
            "enText": "Room for dessert? You mean I can have<n>some?",
            "nextBlock": 18,
            "origClipLength": 75,
            "voiceIdx": 21,
            "voiceSheetId": "051030001",
            "choices": [
                {
                    "jpText": "食べられる分だけね",
                    "enText": "Just don't eat too much.",
                    "nextBlock": 18
                }
            ],
            "animData": [
                {
                    "origLen": 111,
                    "pathId": -8170835387141939383
