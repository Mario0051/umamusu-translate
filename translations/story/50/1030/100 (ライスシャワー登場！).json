                {
                    "origLen": 21,
                    "pathId": 7346649718003874132
                }
            ],
            "pathId": 7780129162628930117,
            "blockIdx": 8
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Umamusume A",
            "jpText": "キャーッ！！",
            "enText": "Hyaaa!",
            "nextBlock": 10,
            "origClipLength": 48,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 59,
                    "pathId": 1887510141141007977
                },
                {
                    "origLen": 59,
                    "pathId": 8558947493763638705
                }
            ],
            "pathId": 1601914140858413149,
            "blockIdx": 9
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "きゃぅっ！？",
            "enText": "Hyaaa?!",
            "nextBlock": 11,
            "origClipLength": 48,
            "voiceIdx": 33,
            "voiceSheetId": "051030001",
            "animData": [
                {
                    "origLen": 59,
