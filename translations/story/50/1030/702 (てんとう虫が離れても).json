            "pathId": -888422453080831284,
            "blockIdx": 16
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "こんな風に考えちゃう、ライス自身。",
            "enText": "Is the fact that I always end up having<n>these\nthoughts.",
            "nextBlock": 18,
            "origClipLength": 56,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 67,
                    "pathId": -6088620993795131007
                },
                {
                    "origLen": 30,
                    "pathId": 6202479160296046919
                }
