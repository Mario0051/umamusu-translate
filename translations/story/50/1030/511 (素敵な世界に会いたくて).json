            "pathId": -5971246075270860436,
            "blockIdx": 4
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "一緒に、来てくれるの？\r\nお姉さまとだったら、うれしい……！",
            "enText": "You want to come with me? I'd love that!",
            "nextBlock": 6,
            "origClipLength": 64,
            "voiceIdx": 27,
            "voiceSheetId": "051030001",
            "animData": [
                {
                    "origLen": 75,
