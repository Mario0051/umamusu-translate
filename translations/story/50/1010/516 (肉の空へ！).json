            "blockIdx": 4
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "だからオスソワケしてみんなハッピー！\r\nイッツ、オモテナシ・スピリッツ！",
            "enText": "Food like this is best served with a whole<n>bunch\nof folks diggin' in. That's that<n>famous Japanese\nhospitality!",
            "nextBlock": 6,
            "origClipLength": 68,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 79,
                    "pathId": 3880211155538351899
