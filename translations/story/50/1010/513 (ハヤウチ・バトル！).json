                {
                    "origLen": 35,
                    "pathId": 8908930921818150366
                }
            ],
            "pathId": -8873395216434420808,
            "blockIdx": 12
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（キーンコーンカーンコーン……）",
            "enText": "<i>Ding-dong, bing-bong...</i>",
            "nextBlock": 14,
            "origClipLength": 93,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "――BANG！！！",
