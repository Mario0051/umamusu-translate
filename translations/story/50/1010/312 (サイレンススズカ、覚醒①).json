            "blockIdx": 1
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "ハーウディー！！",
            "enText": "Howdy!",
            "nextBlock": 3,
            "origClipLength": 65,
            "voiceIdx": 42,
            "voiceSheetId": "051010001",
            "animData": [
                {
                    "origLen": 61,
