            "origClipLength": 56,
            "voiceIdx": 56,
            "voiceSheetId": "051010001",
            "choices": [
                {
                    "jpText": "買ったばかりじゃ……？",
                    "enText": "Didn't you just buy one?",
                    "nextBlock": 16
