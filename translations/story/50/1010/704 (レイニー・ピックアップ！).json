                {
                    "origLen": 28,
                    "pathId": 528281860241742014
                }
            ],
            "pathId": 3568424351248730106,
            "blockIdx": 9
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "フフッ、それでは一緒に帰りまショウ！",
            "enText": "Hehe, all right! Let's head back<n>together!",
            "nextBlock": 11,
            "origClipLength": 56,
            "voiceIdx": 2,
            "voiceSheetId": "051010001",
            "animData": [
                {
                    "origLen": 67,
                    "pathId": -7121833129797577888
