            ],
            "pathId": -314369342019351308,
            "blockIdx": 4
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "最近フレンズに貸りたコミックが面白くて、\r\nつい――",
            "enText": "My friend lent me this comic, and it's a<n>real\nknee-slapper...",
            "nextBlock": 6,
            "origClipLength": 61,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 72,
