            "pathId": -923172741444519814,
            "blockIdx": 2
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（……ポツ、ポツ）",
            "enText": "<i>Plip, plop...</i>",
            "nextBlock": 4,
            "origClipLength": 50,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 61,
