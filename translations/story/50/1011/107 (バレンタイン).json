            "origClipLength": 67,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 78,
                    "pathId": 4089466503755747166
                },
                {
                    "origLen": 58,
                    "pathId": -947617970612433293
                }
