            "pathId": -5495145266984472767,
            "blockIdx": 4
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "私を『有馬記念』の舞台に。",
            "enText": "Take me to the Arima Kinen.",
            "nextBlock": 6,
            "origClipLength": 53,
            "voiceIdx": 72,
            "voiceSheetId": "051011001",
            "animData": [
                {
                    "origLen": 64,
                    "pathId": 6582449245140662851
