            "origClipLength": 69,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 80,
                    "pathId": 4920493140023483770
                },
                {
                    "origLen": 80,
                    "pathId": 3947090850723761197
                }
