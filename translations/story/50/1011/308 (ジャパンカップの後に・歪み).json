            "pathId": 1613100621247587695,
            "blockIdx": 18
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "――だから、アタシでなければ。",
            "enText": "It's gotta be me.",
            "nextBlock": 20,
            "origClipLength": 54,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 14,
