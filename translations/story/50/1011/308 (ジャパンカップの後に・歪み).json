            "voiceIdx": 20,
            "voiceSheetId": "051011001",
            "choices": [
                {
                    "jpText": "グラス？どうした？",
                    "enText": "Is something wrong?",
                    "nextBlock": 11
                },
                {
                    "jpText": "グラス？どうしたの？",
                    "enText": "Is something wrong?",
                    "nextBlock": 11
                }
