        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "まあ、よろしいのですか？",
            "enText": "Oh, are you sure?",
            "nextBlock": 7,
            "origClipLength": 60,
            "voiceIdx": 5,
            "voiceSheetId": "051011001",
            "animData": [
                {
                    "origLen": 66,
