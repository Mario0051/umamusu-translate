            "pathId": 3641014102159538561,
            "blockIdx": 11
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "……ありがとうございます。",
            "enText": "...Thank you.",
            "nextBlock": 13,
            "origClipLength": 53,
            "voiceIdx": 49,
            "voiceSheetId": "051011001",
            "animData": [
                {
                    "origLen": 64,
                    "pathId": -770450422929139242
