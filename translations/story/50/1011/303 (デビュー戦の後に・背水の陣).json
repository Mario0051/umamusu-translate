            "origClipLength": 70,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 81,
                    "pathId": 7424764111340875855
                },
                {
                    "origLen": 61,
                    "pathId": 4579209642007240898
                }
