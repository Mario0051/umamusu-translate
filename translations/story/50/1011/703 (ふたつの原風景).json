            "pathId": -48242235074813745,
            "blockIdx": 1
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "みなさん、お久しぶりです～。",
            "enText": "Hello, everyone. It's been a while.",
            "nextBlock": 3,
            "origClipLength": 54,
            "voiceIdx": 42,
            "voiceSheetId": "051011001",
            "animData": [
                {
                    "origLen": 65,
                    "pathId": 1825873005086814774
