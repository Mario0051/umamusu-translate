            "origClipLength": 80,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 91,
                    "pathId": -6693698658330524161
                },
                {
                    "origLen": 91,
                    "pathId": 462775572573653491
                }
