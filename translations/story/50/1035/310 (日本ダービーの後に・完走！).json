            "origClipLength": 68,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 79,
                    "pathId": 8765146049000936538
                },
                {
                    "origLen": 41,
                    "pathId": 8330748674344560332
                }
