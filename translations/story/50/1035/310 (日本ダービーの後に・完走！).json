            "enName": "Winning Ticket",
            "jpText": "ぐず……。",
            "enText": "<i>Sniffle...</i>",
            "nextBlock": 11,
            "origClipLength": 51,
            "voiceIdx": 22,
            "voiceSheetId": "051035001",
            "choices": [
                {
                    "jpText": "落ち着いた？",
                    "enText": "Have you calmed down a bit?",
                    "nextBlock": 11
                }
            ],
            "animData": [
                {
                    "origLen": 62,
                    "pathId": -4829003302035976047
