            "origClipLength": 86,
            "voiceIdx": 79,
            "voiceSheetId": "051035001",
            "choices": [
                {
                    "jpText": "お返ししてないのか？",
                    "enText": "So you didn't give your friends anything?",
                    "nextBlock": 15
