            "origClipLength": 90,
            "voiceIdx": 9,
            "voiceSheetId": "051035001",
            "animData": [
                {
                    "origLen": 101,
                    "pathId": 4086352616817802728
                },
                {
                    "origLen": 79,
                    "pathId": 8884450507321576258
                }
            ],
            "pathId": -4960573029551047251,
            "blockIdx": 25
        },
