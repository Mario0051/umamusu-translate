            "nextBlock": 23,
            "origClipLength": 91,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "明日のトレーニングにぶつけよう！",
                    "enText": "Save that motivation for tomorrow's session!",
                    "nextBlock": 23
