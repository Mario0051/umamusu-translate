            "nextBlock": 22,
            "origClipLength": 93,
            "voiceIdx": 0,
            "voiceSheetId": "051035001",
            "choices": [
                {
                    "jpText": "（インスピレーションのことかな……）",
                    "enText": "(Did she mean \"flash\" of inspiration?)",
                    "nextBlock": 22
                }
