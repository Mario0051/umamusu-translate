            "origClipLength": 90,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 101,
                    "pathId": -6807156254359419920
                },
                {
                    "origLen": 101,
                    "pathId": -8639840533785147653
                }
