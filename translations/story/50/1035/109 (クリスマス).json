            "pathId": 1232420102388802069,
            "blockIdx": 18
        },
        {
            "jpName": "ウイニングチケット",
            "enName": "Winning Ticket",
            "jpText": "じゃじゃじゃじゃ～～～んッ！\r\n雪だるま型クリスマスケーキ～～～！！",
            "enText": "Ta-da-DAAA! A snowman-shaped cake!",
            "nextBlock": 20,
            "origClipLength": 89,
            "voiceIdx": 149,
            "voiceSheetId": "051035001",
            "animData": [
                {
                    "origLen": 100,
