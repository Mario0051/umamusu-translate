            "blockIdx": 22
        },
        {
            "jpName": "ウイニングチケット",
            "enName": "Winning Ticket",
            "jpText": "心の奥がぎゅう～～～ってなるんだよ～～！",
            "enText": "I can feel something squeezing inside my<n>chest,\nyou know?",
            "nextBlock": 24,
            "origClipLength": 71,
            "voiceIdx": 10,
            "voiceSheetId": "051035001",
            "animData": [
                {
                    "origLen": 82,
                    "pathId": 3754940479502908177
