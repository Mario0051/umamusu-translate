            "pathId": -4673662003859426938,
            "blockIdx": 18
        },
        {
            "jpName": "ウイニングチケット",
            "enName": "Winning Ticket",
            "jpText": "ダービーーー！！　サイコォーーーーーッ！！",
            "enText": "DERBYYY! You're the BEEEEST!",
            "nextBlock": 20,
            "origClipLength": 72,
            "voiceIdx": 10,
            "voiceSheetId": "051035001",
            "animData": [
                {
                    "origLen": 83,
                    "pathId": 160392183639595088
                },
                {
                    "origLen": 48,
                    "pathId": -1461710957625013134
                }
            ],
            "pathId": 6387945445631905893,
            "blockIdx": 19
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "ウイニングチケットも負けず劣らず\r\n大変な盛り上がりを見せていた……！",
            "enText": "...Winning Ticket seems every bit as<n>emotional as\nthe runners on the track!",
            "nextBlock": 21,
            "origClipLength": 90,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "やっぱりダービーが大好きなんだな",
                    "enText": "You really love the Derby, huh?",
                    "nextBlock": 21
                },
                {
                    "jpText": "やっぱりダービーが大好きなんだね",
                    "enText": "You really love the Derby, huh?",
                    "nextBlock": 21
                }
