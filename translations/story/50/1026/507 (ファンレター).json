            "pathId": -4198108846683757106,
            "blockIdx": 13
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "……お願いします、マスター。",
            "enText": "Please allow me to continue.",
            "nextBlock": 15,
            "origClipLength": 63,
            "voiceIdx": 39,
            "voiceSheetId": "051026001",
            "animData": [
                {
                    "origLen": 74,
