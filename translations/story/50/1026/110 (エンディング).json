                },
                {
                    "origLen": 42,
                    "pathId": 3369873318386378886
                }
            ],
            "pathId": 4099360165417732757,
            "blockIdx": 17
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "…………。\n…………。",
            "enText": "...",
            "nextBlock": 19,
            "origClipLength": 59,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "（……つらい！）",
                    "enText": "(...This is tough!)",
                    "nextBlock": 19
