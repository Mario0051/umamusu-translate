        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "……負けた……！",
            "enText": "I lost!",
            "nextBlock": 5,
            "origClipLength": 55,
            "voiceIdx": 66,
            "voiceSheetId": "051026001",
            "animData": [
                {
                    "origLen": 110,
