            "voiceIdx": 15,
            "voiceSheetId": "051026001",
            "choices": [
                {
                    "jpText": "いい走りだった、ブルボン！",
                    "enText": "Great job out there, Bourbon!",
                    "nextBlock": 8
                },
                {
                    "jpText": "いい走りだったよ、ブルボン！",
                    "enText": "Great job out there, Bourbon!",
                    "nextBlock": 8
