                },
                {
                    "origLen": 52,
                    "pathId": -6432481021506925668
                }
            ],
            "pathId": -2905589739620781740,
            "blockIdx": 16
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "……光栄です。",
            "enText": "I'm honored to hear that.",
            "nextBlock": 18,
            "origClipLength": 54,
            "voiceIdx": 0,
            "voiceSheetId": "051026001",
            "animData": [
                {
                    "origLen": 67,
                    "pathId": -3069245128708624505
                },
                {
                    "origLen": 66,
                    "pathId": -4871506418123746779
