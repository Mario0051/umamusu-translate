            "blockIdx": 20
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "マスター。\nそれでは、行ってきます。",
            "enText": "I'll be off now, Trainer.",
            "nextBlock": 22,
            "origClipLength": 68,
            "voiceIdx": 138,
            "voiceSheetId": "051026001",
