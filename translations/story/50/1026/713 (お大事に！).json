            "origClipLength": 65,
            "voiceIdx": 124,
            "voiceSheetId": "051026001",
            "choices": [
                {
                    "jpText": "待て！50％は危な――",
                    "enText": "Wait! 50% is too—",
                    "nextBlock": 12
                },
                {
                    "jpText": "待って！50％は危な――",
                    "enText": "Wait! 50% is too—",
                    "nextBlock": 12
