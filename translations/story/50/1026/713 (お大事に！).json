        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "……～～ッ！\r\n患部の、痛みを……確認……。",
            "enText": "Pain in... damaged area... confirmed...",
            "nextBlock": 15,
            "origClipLength": 73,
            "voiceIdx": 108,
            "voiceSheetId": "051026001",
            "choices": [
                {
                    "jpText": "大丈夫か！？",
                    "enText": "Are you all right?!",
                    "nextBlock": 15
                },
                {
                    "jpText": "大丈夫！？",
                    "enText": "Are you all right?!",
                    "nextBlock": 15
