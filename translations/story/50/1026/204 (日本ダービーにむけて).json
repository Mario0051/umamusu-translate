                {
                    "origLen": 70,
                    "pathId": -8833146351006642506
                }
            ],
            "pathId": 2709707143661298913,
            "blockIdx": 11
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（コンコンッ）",
            "enText": "<i>Knock, knock.</i>",
            "nextBlock": 13,
            "origClipLength": 54,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 71,
