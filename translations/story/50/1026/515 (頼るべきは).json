            "pathId": 9157793117943294428,
            "blockIdx": 23
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "……？",
            "enText": "...?",
            "nextBlock": 25,
            "origClipLength": 48,
