            "pathId": 5253651956911408979,
            "blockIdx": 2
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "はい。実は――",
            "enText": "Yes. You see—",
            "nextBlock": 4,
            "origClipLength": 54,
            "voiceIdx": 37,
            "voiceSheetId": "051026001",
            "animData": [
                {
                    "origLen": 16,
                    "pathId": 4574919478844869273
                },
                {
                    "origLen": 65,
                    "pathId": -8354819173751825889
