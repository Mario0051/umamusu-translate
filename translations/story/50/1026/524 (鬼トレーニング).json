            "blockIdx": 7
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "……マスター？",
            "enText": "Trainer?",
            "nextBlock": 9,
            "origClipLength": 54,
            "voiceIdx": 26,
            "voiceSheetId": "051026001",
            "choices": [
                {
                    "jpText": "頑張れ！ブルボン！",
                    "enText": "Keep at it, Bourbon!",
                    "nextBlock": 9
                }
            ],
            "animData": [
                {
                    "origLen": 65,
                    "pathId": -8315780735212915871
                },
                {
                    "origLen": 36,
                    "pathId": -7269432868359125352
