            "pathId": 2344392796816647152,
            "blockIdx": 10
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "マスター……。\n申し訳ございません……。",
            "enText": "Trainer... Please accept my apologies...",
            "nextBlock": 12,
            "origClipLength": 71,
            "voiceIdx": 24,
            "voiceSheetId": "051026001",
