            "blockIdx": 4
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ウワァアアアアア――！）",
            "enText": "Wooooo!",
            "nextBlock": 6,
            "origClipLength": 61,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 21,
