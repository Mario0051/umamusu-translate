            "blockIdx": 10
        },
        {
            "jpName": "スマートファルコン",
            "enName": "Smart Falcon",
            "jpText": "へ……なんのこと？",
            "enText": "Huh? For what?",
            "nextBlock": 12,
            "origClipLength": 56,
            "voiceIdx": 20,
            "voiceSheetId": "051046001",
            "animData": [
                {
                    "origLen": 67,
                    "pathId": 7520865383338289692
