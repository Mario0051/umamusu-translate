            "nextBlock": 18,
            "origClipLength": 123,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "芝にまだ未練があるのか？",
                    "enText": "You're still attached to running on turf?",
                    "nextBlock": 18
