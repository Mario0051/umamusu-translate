            "blockIdx": 1
        },
        {
            "jpName": "スマートファルコン",
            "enName": "Smart Falcon",
            "jpText": "はぁ～……！\n始まるね……！",
            "enText": "Wow... It's happening!",
            "nextBlock": 3,
            "origClipLength": 63,
            "voiceIdx": 59,
            "voiceSheetId": "051046001",
            "choices": [
                {
                    "jpText": "緊張してる？",
