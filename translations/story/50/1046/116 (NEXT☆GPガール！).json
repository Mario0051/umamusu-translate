            "nextBlock": 7,
            "origClipLength": 77,
            "voiceIdx": 113,
            "voiceSheetId": "051046001",
            "choices": [
                {
                    "jpText": "年末の一大レースだからな",
                    "enText": "Well, it is <i>the</i> huge end-of-year race.",
                    "nextBlock": 7
                },
                {
                    "jpText": "年末の一大レースだからね",
                    "enText": "Well, it is <i>the</i> huge end-of-year race.",
