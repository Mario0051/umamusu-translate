            "blockIdx": 5
        },
        {
            "jpName": "スマートファルコン",
            "enName": "Smart Falcon",
            "jpText": "そうだよねっ！",
            "enText": "You're right!",
            "nextBlock": 7,
            "origClipLength": 54,
            "voiceIdx": 149,
            "voiceSheetId": "051046001",
            "animData": [
