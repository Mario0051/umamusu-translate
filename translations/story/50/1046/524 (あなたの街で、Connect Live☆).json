            "origClipLength": 77,
            "voiceIdx": 149,
            "voiceSheetId": "051046001",
            "animData": [
                {
                    "origLen": 88,
                    "pathId": 8526799845628938546
                },
                {
                    "origLen": 54,
                    "pathId": 475831007993373430
                }
