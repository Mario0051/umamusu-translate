            ],
            "pathId": 6489880401039841885,
            "blockIdx": 23
        },
        {
            "jpName": "スマートファルコン",
            "enName": "Smart Falcon",
            "jpText": "もっちろん☆\r\nだって……。",
            "enText": "Of course! ☆ After all...",
            "nextBlock": 25,
            "origClipLength": 63,
            "voiceIdx": 149,
            "voiceSheetId": "051046001",
            "animData": [
                {
                    "origLen": 74,
