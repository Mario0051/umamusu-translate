            "animData": [
                {
                    "origLen": 20,
                    "pathId": 3426058791940704868
                },
                {
                    "origLen": 92,
                    "pathId": -683201250798476492
                },
                {
                    "origLen": 90,
                    "pathId": 8444764820716964247
