            "nextBlock": 10,
            "origClipLength": 58,
            "voiceIdx": 69,
            "voiceSheetId": "051024001",
            "choices": [
                {
                    "jpText": "君なら必ず叶えられる",
                    "enText": "I'm sure your wish will come true.",
                    "nextBlock": 10
                },
                {
                    "jpText": "貴方なら必ず叶えられるよ",
                    "enText": "I'm sure your wish will come true.",
                    "nextBlock": 10
