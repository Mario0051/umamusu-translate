                }
            ],
            "pathId": 415035093474230655,
            "blockIdx": 1
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "ああ、もう！\r\nムカツク……！",
            "enText": "Nnngh, dammit! I'm gonna freakin'<n>EXPLODE!",
            "nextBlock": 3,
            "origClipLength": 64,
            "voiceIdx": 17,
            "voiceSheetId": "051050001",
            "choices": [
                {
                    "jpText": "何かあったのか？",
                    "enText": "What happened?",
                    "nextBlock": 3
                },
                {
                    "jpText": "何かあったの？",
                    "enText": "What happened?",
                    "nextBlock": 3
                }
