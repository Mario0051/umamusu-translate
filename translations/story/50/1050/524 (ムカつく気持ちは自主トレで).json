            "origClipLength": 82,
            "voiceIdx": 26,
            "voiceSheetId": "051050001",
            "choices": [
                {
                    "jpText": "今の気持ちはレースにぶつけるといい",
                    "enText": "You should channel that energy into your next\nrace.",
                    "nextBlock": 27
                },
                {
                    "jpText": "今の気持ちはレースにぶつけるといいよ",
                    "enText": "You should channel that energy into your next\nrace.",
                    "nextBlock": 27
                }
