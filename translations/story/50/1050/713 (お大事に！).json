            "pathId": 5514411214934414846,
            "blockIdx": 14
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "はぁ、はぁ……。",
            "enText": "<i>Pant... pant...</i>",
            "nextBlock": 16,
            "origClipLength": 97,
            "voiceIdx": 58,
            "voiceSheetId": "051050001",
            "animData": [
                {
                    "origLen": 141,
                    "pathId": 7194358227861048339
                },
                {
                    "origLen": 127,
                    "pathId": 5911773258932160726
                }
