            "pathId": -3063952536848357424,
            "blockIdx": 15
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "――ッ！！",
            "enText": "...!",
            "nextBlock": 17,
            "origClipLength": 51,
            "voiceIdx": 108,
            "voiceSheetId": "051050001",
            "choices": [
                {
                    "jpText": "やっぱりさっきのケガが……",
                    "enText": "Okay, clearly that injury is affecting—",
                    "nextBlock": 17
