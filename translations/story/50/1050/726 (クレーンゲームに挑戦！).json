            "pathId": -5019786124330260073,
            "blockIdx": 1
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "……あれって。",
            "enText": "Hang on.",
            "nextBlock": 3,
            "origClipLength": 54,
            "voiceIdx": 31,
            "voiceSheetId": "051050001",
            "choices": [
                {
                    "jpText": "どうしたんだ？",
