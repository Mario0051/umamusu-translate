            "pathId": -6903366867734757593,
            "blockIdx": 15
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "……あ、そ。",
            "enText": "Uh... okay.",
            "nextBlock": 17,
            "origClipLength": 81,
            "voiceIdx": 28,
            "voiceSheetId": "051050001",
            "animData": [
                {
                    "origLen": 92,
                    "pathId": -591217769144542377
