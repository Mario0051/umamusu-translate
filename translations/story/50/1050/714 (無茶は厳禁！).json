            "blockIdx": 4
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "……な、何！？",
            "enText": "What?!",
            "nextBlock": 6,
            "origClipLength": 54,
            "voiceIdx": 110,
            "voiceSheetId": "051050001",
            "choices": [
                {
                    "jpText": "問題ありそうだから保健室に来たんだ",
                    "enText": "We're here <i>precisely</i> because something\nappears to be wrong.",
                    "nextBlock": 7
                },
                {
                    "jpText": "問題ありそうだから保健室に来たんだよ",
                    "enText": "We're here <i>precisely</i> because something\nappears to be wrong.",
                    "nextBlock": 7
                },
                {
                    "jpText": "君の熱意に応えるよ！",
                    "enText": "I'll support you all the way!",
                    "nextBlock": 15
                },
                {
                    "jpText": "貴方の熱意に応えるよ！",
                    "enText": "I'll support you all the way!",
                    "nextBlock": 15
