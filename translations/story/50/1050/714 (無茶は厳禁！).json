                },
                {
                    "origLen": 43,
                    "pathId": -8422329269789281663
                }
            ],
            "pathId": -8791147625364046030,
            "blockIdx": 3
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "トレーニングだって、\r\n問題なくできるし――",
            "enText": "There's nothing wrong so I can jump<n>right back\ninto trainin—",
            "nextBlock": 5,
            "origClipLength": 73,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "タイシン！",
                    "enText": "Taishin!",
                    "nextBlock": 5
                }
            ],
            "animData": [
                {
                    "origLen": 84,
