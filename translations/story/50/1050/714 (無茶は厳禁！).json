            "pathId": 7853391997867829397,
            "blockIdx": 17
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "はぁ、はぁ……！",
            "enText": "<i>Pant... pant...</i>",
            "nextBlock": 19,
            "origClipLength": 97,
            "voiceIdx": 58,
            "voiceSheetId": "051050001",
            "choices": [
                {
                    "jpText": "いけいけタイシーン！！",
                    "enText": "You've got this, Taishiiin!",
                    "nextBlock": 19
                }
