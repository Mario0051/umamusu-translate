            "pathId": -8520157496808701774,
            "blockIdx": 19
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "いったぁ……っ！？",
            "enText": "OUCH!",
            "nextBlock": 21,
            "origClipLength": 56,
            "voiceIdx": 109,
            "voiceSheetId": "051050001",
            "animData": [
                {
                    "origLen": 13,
