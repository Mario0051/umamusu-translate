            "pathId": -4384193885600478617,
            "blockIdx": 9
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "……げ。",
            "enText": "...Ugh.",
            "nextBlock": 11,
            "origClipLength": 50,
            "voiceIdx": 78,
            "voiceSheetId": "051050001",
            "animData": [
