            "blockIdx": 9
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "ふーん……静かだし……\r\n昼寝するにもよさそうじゃん。",
            "enText": "Huh. This might be a decent spot to nap<n>since it's\nso quiet.",
            "nextBlock": 11,
            "origClipLength": 80,
            "voiceIdx": 8,
            "voiceSheetId": "051050001",
            "animData": [
                {
                    "origLen": 91,
                    "pathId": -6616124179683514527
                },
                {
                    "origLen": 41,
                    "pathId": 2085378794820515973
                }
