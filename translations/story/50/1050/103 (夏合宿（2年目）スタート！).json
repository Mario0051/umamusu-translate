            "nextBlock": 4,
            "origClipLength": 54,
            "voiceIdx": 60,
            "voiceSheetId": "051050001",
            "choices": [
                {
                    "jpText": "だ、大丈夫か？",
                    "enText": "Y-you okay?",
                    "nextBlock": 4
                },
                {
                    "jpText": "だ、大丈夫？",
                    "enText": "Y-you okay?",
                    "nextBlock": 4
