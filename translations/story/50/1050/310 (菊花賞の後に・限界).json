            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "タイシン、大丈夫か！？",
                    "enText": "Are you okay, Taishin?!",
                    "nextBlock": 6
                },
                {
                    "jpText": "タイシン、大丈夫！？",
                    "enText": "Are you okay, Taishin?!",
                    "nextBlock": 6
                }
