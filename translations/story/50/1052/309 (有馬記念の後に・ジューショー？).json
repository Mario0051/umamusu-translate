                {
                    "origLen": 84,
                    "pathId": 5030607194566275371
                },
                {
                    "origLen": 84,
                    "pathId": 3620947269292018007
                }
            ],
            "pathId": 2873713971813543598,
            "blockIdx": 19
        }
    ]
}