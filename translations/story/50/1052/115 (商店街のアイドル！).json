            "blockIdx": 8
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "話は数日前に遡る――",
            "enText": "Several days earlier...",
            "nextBlock": 10,
            "origClipLength": 51,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 62,
