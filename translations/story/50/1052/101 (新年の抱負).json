            "nextBlock": 6,
            "origClipLength": 59,
            "voiceIdx": 37,
            "voiceSheetId": "051052001",
            "choices": [
                {
                    "jpText": "抱負は決めてきたか？",
                    "enText": "Have you made any resolutions?",
                    "nextBlock": 6
                },
                {
                    "jpText": "抱負は決めてきた？",
                    "enText": "Have you made any resolutions?",
                    "nextBlock": 6
