            ],
            "pathId": 4634966333109462848,
            "blockIdx": 3
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "とにかく、おめでとう！\r\n今年もよろしくねー！",
            "enText": "Anyhoo, Happy New Year! Let's have a<n>great year\ntogether!",
            "nextBlock": 5,
            "origClipLength": 60,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "よろしく！",
                    "enText": "Yeah!",
                    "nextBlock": 5
