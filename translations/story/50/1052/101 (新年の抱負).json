            "blockIdx": 18
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "気合い……！\r\n気合いって、そんなすごいの？",
            "enText": "A fighting spirit! Is it really that strong?",
            "nextBlock": 20,
            "origClipLength": 59,
            "voiceIdx": 26,
            "voiceSheetId": "051052001",
            "animData": [
                {
                    "origLen": 70,
                    "pathId": -499527858272917630
                },
                {
                    "origLen": 46,
                    "pathId": -1344251716404156702
                }
