            "animData": [
                {
                    "origLen": 78,
                    "pathId": 8093780285112804808
                },
                {
                    "origLen": 42,
                    "pathId": 3848842787966484083
                }
            ],
            "pathId": -2519491943230687446,
            "blockIdx": 7
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "目標か～！\r\nだったらね、あるよ！",
            "enText": "Oh, a goal! I've got one of those!",
            "nextBlock": 9,
            "origClipLength": 56,
            "voiceIdx": 73,
            "voiceSheetId": "051052001",
            "animData": [
