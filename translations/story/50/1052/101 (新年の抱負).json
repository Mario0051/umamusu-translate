            "nextBlock": 27,
            "origClipLength": 61,
            "voiceIdx": 20,
            "voiceSheetId": "051052001",
            "choices": [
                {
                    "jpText": "じゃあ、いつもよりもっと寝よう！",
                    "enText": "Then you need to sleep even more!",
                    "nextBlock": 27
                }
