            "origClipLength": 60,
            "voiceIdx": 52,
            "voiceSheetId": "051052001",
            "choices": [
                {
                    "jpText": "次の楽しみに取っておこう",
                    "enText": "Let's save it for another day.",
                    "nextBlock": 6
                },
                {
                    "jpText": "走っていこう！",
                    "enText": "We gotta book it!",
