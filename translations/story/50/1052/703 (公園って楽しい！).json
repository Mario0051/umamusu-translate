            "blockIdx": 9
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "一緒に遊ぼうよ～！\nすっごい楽しいよ～！",
            "enText": "Come on! It's super fun!",
            "nextBlock": 11,
            "origClipLength": 58,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "大丈夫",
                    "enText": "No, I'm okay.",
                    "nextBlock": 11
