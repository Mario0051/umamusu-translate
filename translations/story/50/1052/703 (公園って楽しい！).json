            "pathId": 6762263202804846569,
            "blockIdx": 21
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "え！？　なんで！？",
            "enText": "What? Why?!",
            "nextBlock": 23,
            "origClipLength": 50,
            "voiceIdx": 26,
            "voiceSheetId": "051052001",
            "choices": [
                {
                    "jpText": "もう大人だから",
                    "enText": "I'm not a kid anymore.",
                    "nextBlock": 23
