            "pathId": 3279672521656924870,
            "blockIdx": 18
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Umamusume A",
            "jpText": "……。",
            "enText": "Hmph.",
            "nextBlock": 20,
            "origClipLength": 46,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 57,
                    "pathId": -849122852369314313
