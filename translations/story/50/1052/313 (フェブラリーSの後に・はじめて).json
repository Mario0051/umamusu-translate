            "pathId": -1634636868864039676,
            "blockIdx": 17
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "うん――あれ？",
            "enText": "Yeah, I wi―huh?",
            "nextBlock": 19,
            "origClipLength": 49,
            "voiceIdx": 30,
            "voiceSheetId": "051052001",
            "animData": [
                {
                    "origLen": 60,
