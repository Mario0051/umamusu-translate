            "pathId": -5326244885376180049,
            "blockIdx": 19
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "トレーナー……もう寝た？",
            "enText": "Are you... asleep?",
            "nextBlock": 21,
            "origClipLength": 52,
            "voiceIdx": 30,
            "voiceSheetId": "051052001",
            "choices": [
                {
                    "jpText": "起きてる",
