            "pathId": 6768695313375027125,
            "blockIdx": 20
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "……ねえねえ、もう寝ちゃった？",
            "enText": "...Are you asleep <i>now?</i>",
            "nextBlock": 22,
            "origClipLength": 54,
            "voiceIdx": 52,
            "voiceSheetId": "051052001",
            "choices": [
                {
                    "jpText": "まだ起きてる",
                    "enText": "Nope.",
                    "nextBlock": 22
