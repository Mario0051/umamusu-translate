            "blockIdx": 3
        },
        {
            "jpName": "後輩ウマ娘",
            "enName": "Younger Student",
            "jpText": "………………。",
            "enText": "...",
            "nextBlock": 5,
            "origClipLength": 49,
            "voiceIdx": -1,
