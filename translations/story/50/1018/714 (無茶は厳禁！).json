                {
                    "origLen": 21,
                    "pathId": -1002112471195393001
                }
            ],
            "pathId": -1444913163390152808,
            "blockIdx": 6
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "うっ……。",
            "enText": "Ngh...",
            "nextBlock": 8,
            "origClipLength": 48,
            "voiceIdx": 108,
            "voiceSheetId": "051018001",
            "choices": [
                {
                    "jpText": "体を動かしちゃダメだ",
                    "enText": "You shouldn't move around too much.",
                    "nextBlock": 8
                },
                {
                    "jpText": "体を動かしちゃダメだよ",
                    "enText": "You shouldn't move around too much.",
                    "nextBlock": 8
                }
            ],
            "animData": [
                {
                    "origLen": 59,
                    "pathId": -3348246877482839861
                },
                {
                    "origLen": 26,
                    "pathId": 3732047000433766455
                }
