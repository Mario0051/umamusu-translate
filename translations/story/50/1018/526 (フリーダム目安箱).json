        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "む……貴様か。",
            "enText": "Oh, hello.",
            "nextBlock": 6,
            "origClipLength": 59,
            "voiceIdx": 37,
            "voiceSheetId": "051018001",
            "animData": [
                {
