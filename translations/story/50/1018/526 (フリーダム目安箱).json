        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "……貴様、暇なのか？",
            "enText": "Looking for something to do?",
            "nextBlock": 17,
            "origClipLength": 55,
            "voiceIdx": 88,
            "voiceSheetId": "051018001",
            "animData": [
                {
                    "origLen": 90,
                    "pathId": 2442908829468707975
