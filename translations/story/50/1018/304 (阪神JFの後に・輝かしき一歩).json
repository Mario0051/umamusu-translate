            "pathId": -3356693865974505415,
            "blockIdx": 8
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "――おめでとう、エアグルーヴ。",
            "enText": "Well done, Air Groove.",
            "nextBlock": 10,
            "origClipLength": 54,
            "voiceIdx": 129,
            "voiceSheetId": "051017001",
            "animData": [
                {
                    "origLen": 65,
                    "pathId": 3509166941482704797
                },
                {
                    "origLen": 65,
                    "pathId": -1806578671826190180
