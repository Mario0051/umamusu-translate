            "pathId": -8442368735060057086,
            "blockIdx": 16
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "……ふっ。",
            "enText": "...Heh.",
            "nextBlock": 18,
            "origClipLength": 48,
            "voiceIdx": 0,
            "voiceSheetId": "051018001",
            "choices": [
                {
                    "jpText": "よかったな",
