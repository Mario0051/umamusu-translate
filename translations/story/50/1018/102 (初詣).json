            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 76,
                    "pathId": -417195170124498619
                },
                {
                    "origLen": 76,
                    "pathId": -3300802551045097396
