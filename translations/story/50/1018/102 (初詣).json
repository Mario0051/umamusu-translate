            "pathId": 7603271686652718444,
            "blockIdx": 24
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "言うようになりおって……。",
            "enText": "I can't believe you're using my own<n>words against\nme...",
            "nextBlock": 26,
            "origClipLength": 53,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 64,
