            "blockIdx": 1
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "……貴様には関係ないだろう。\r\n詮索は無用だ。",
            "enText": "This doesn't concern you. Don't stick<n>your nose\nwhere it doesn't belong.",
            "nextBlock": 3,
            "origClipLength": 60,
            "voiceIdx": 60,
            "voiceSheetId": "051018001",
            "choices": [
                {
                    "jpText": "体調が悪いんじゃない？",
                    "enText": "You don't look so good.",
                    "nextBlock": 3
                }
            ],
            "animData": [
                {
                    "origLen": 71,
