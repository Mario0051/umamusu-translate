        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "……そこまで心配しなくてもいい。",
            "enText": "...It's not that serious.",
            "nextBlock": 4,
            "origClipLength": 55,
            "voiceIdx": 50,
            "voiceSheetId": "051018001",
