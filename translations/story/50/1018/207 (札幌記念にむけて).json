            "nextBlock": 3,
            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "今日の走りを見るのが楽しみ",
                    "enText": "I'm looking forward to seeing you run.",
