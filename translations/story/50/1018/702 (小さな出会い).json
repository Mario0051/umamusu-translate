            "origClipLength": 69,
            "voiceIdx": 79,
            "voiceSheetId": "051018001",
            "animData": [
                {
                    "origLen": 80,
                    "pathId": 7414403215307386682
                },
                {
                    "origLen": 60,
                    "pathId": 6539801283774662698
