        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "悪くない走りってところかしら。\nまあ、言うならば……。",
            "enText": "Not a bad result, if I do say so myself.",
            "nextBlock": 3,
            "origClipLength": 63,
            "voiceIdx": 79,
            "voiceSheetId": "051061001",
            "choices": [
                {
                    "jpText": "君の才能に結果がついてきた",
                    "enText": "The results demonstrate how talented you are.",
                    "nextBlock": 3
                },
                {
                    "jpText": "貴方の才能に結果がついてきた",
                    "enText": "The results demonstrate how talented you are.",
                    "nextBlock": 3
                }
