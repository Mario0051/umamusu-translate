            "pathId": 7972605384329108542,
            "blockIdx": 9
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "ちょっと失礼。",
            "enText": "Excuse me for a moment.",
            "nextBlock": 11,
            "origClipLength": 49,
            "voiceIdx": 59,
            "voiceSheetId": "051061001",
            "animData": [
                {
                    "origLen": 102,
