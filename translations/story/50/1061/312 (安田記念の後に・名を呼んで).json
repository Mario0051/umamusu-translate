            "blockIdx": 3
        },
        {
            "jpName": "観客A",
            "enName": "Fan A",
            "jpText": "キングヘイロー！！　やるじゃないかー！",
            "enText": "King Halo! You did it!",
            "nextBlock": 5,
            "origClipLength": 57,
            "voiceIdx": -1,
