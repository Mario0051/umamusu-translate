            "blockIdx": 13
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "ごきげんよう、お母さま。",
            "enText": "Hello, Mother.",
            "nextBlock": 15,
            "origClipLength": 52,
            "voiceIdx": 42,
            "voiceSheetId": "051061001",
            "animData": [
                {
                    "origLen": 70,
                    "pathId": -6103383616035501162
