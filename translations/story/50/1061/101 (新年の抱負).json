                {
                    "origLen": 44,
                    "pathId": 1765135683154989357
                }
            ],
            "pathId": 540562020588765559,
            "blockIdx": 7
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（コンコン）",
            "enText": "<i>Knock, knock!</i>",
            "nextBlock": 9,
            "origClipLength": 48,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 59,
                    "pathId": -7581123240602619128
