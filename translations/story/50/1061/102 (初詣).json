            "origClipLength": 56,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "……上着持ってこようか？",
                    "enText": "...Do you want me to get your coat?",
                    "nextBlock": 5
                },
                {
                    "jpText": "……上着貸そうか？",
                    "enText": "...Do you want to borrow my coat?",
                    "nextBlock": 5
