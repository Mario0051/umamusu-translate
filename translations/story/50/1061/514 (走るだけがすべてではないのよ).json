            "pathId": 6288826249965268126,
            "blockIdx": 12
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "そ、それは気が変わったの！",
            "enText": "W-well, I changed my mind!",
            "nextBlock": 14,
            "origClipLength": 53,
            "voiceIdx": 111,
            "voiceSheetId": "051061001",
            "animData": [
