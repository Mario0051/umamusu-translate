        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "ふう……ふう……。",
            "enText": "<i>Huff... huff...</i>",
            "nextBlock": 3,
            "origClipLength": 129,
            "voiceIdx": 58,
            "voiceSheetId": "051061001",
            "animData": [
