            "pathId": -934247132980133548,
            "blockIdx": 21
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "……簡単に、子ども扱いしないで。",
            "enText": "No, you just like to treat me like one...",
            "nextBlock": 23,
            "origClipLength": 55,
            "voiceIdx": 108,
            "voiceSheetId": "051061001",
            "animData": [
                {
                    "origLen": 66,
                    "pathId": 2369271120605312782
                },
                {
                    "origLen": 66,
                    "pathId": -7741033053845214389
                },
                {
                    "origLen": 66,
                    "pathId": 2446934956338458320
                }
