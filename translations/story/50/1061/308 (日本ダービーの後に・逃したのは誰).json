            "blockIdx": 19
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "……ごきげんよう、お母さま。",
            "enText": "...Hello, Mother.",
            "nextBlock": 21,
            "origClipLength": 54,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 65,
