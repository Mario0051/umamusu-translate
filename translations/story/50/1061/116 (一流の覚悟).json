            "blockIdx": 15
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "おーっほっほっほ！　来たわね！\r\nメディアのみなさま方！！",
            "enText": "Ohohoho! The media's all here!",
            "nextBlock": 17,
            "origClipLength": 63,
            "voiceIdx": 1,
            "voiceSheetId": "051061001",
            "animData": [
                {
                    "origLen": 74,
