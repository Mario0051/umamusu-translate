            "blockIdx": 21
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "私の名前は――っ！？",
            "enText": "What's my naaame?!",
            "nextBlock": 23,
            "origClipLength": 51,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "……キングっ！！",
