            "blockIdx": 16
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "では始めましょうか、トレーナー。\r\n『メディア用ごあいさつ』を。",
            "enText": "Trainer! It's time for my media greeting<n>routine!",
            "nextBlock": 18,
            "origClipLength": 65,
            "voiceIdx": 95,
            "voiceSheetId": "051061001",
            "choices": [
                {
                    "jpText": "『メディア用ごあいさつ』？",
                    "enText": "What media greeting routine?",
