            "origClipLength": 76,
            "voiceIdx": 10,
            "voiceSheetId": "051061001",
            "animData": [
                {
                    "origLen": 19,
                    "pathId": 5947257615862262705
                },
                {
                    "origLen": 19,
                    "pathId": -3086096515864097945
                },
                {
                    "origLen": 80,
                    "pathId": 885415924218328175
