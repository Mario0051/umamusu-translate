            "blockIdx": 3
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "ワン、ツー、スリー♪\r\nワン、ツー、スリー♪",
            "enText": "One, two, three! ♪ One, two, three! ♪",
            "nextBlock": 5,
            "origClipLength": 104,
            "voiceIdx": -1,
