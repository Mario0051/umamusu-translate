            "pathId": -1365898714700561677,
            "blockIdx": 4
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "――さぁ、ここでキングコールよ、\r\nさんはいっ！",
            "enText": "You're supposed to chant \"King\" here,<n>okay?\nAaaaand, go!",
            "nextBlock": 6,
            "origClipLength": 60,
            "voiceIdx": 91,
            "voiceSheetId": "051061001",
            "choices": [
                {
                    "jpText": "え……？",
                    "enText": "Huh?",
                    "nextBlock": 6
                }
