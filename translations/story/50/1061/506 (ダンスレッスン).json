    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "キングヘイローに呼ばれて\r\nダンススタジオにやって来た。",
            "enText": "I head to the dance studio at King Halo's<n>request.",
            "nextBlock": 2,
            "origClipLength": 63,
            "voiceIdx": -1,
