            "pathId": 8232579307324790028,
            "blockIdx": 1
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "今日はキングのダンスレッスンを見る\r\n権利をあげるわ。",
            "enText": "Today, you shall have the privilege of<n>watching\nmy royal dance lesson.",
            "nextBlock": 3,
            "origClipLength": 62,
            "voiceIdx": 61,
            "voiceSheetId": "051061001",
            "animData": [
                {
                    "origLen": 73,
                    "pathId": -5828231690523393705
