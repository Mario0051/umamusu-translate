            ],
            "pathId": -7626005282332786367,
            "blockIdx": 5
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "『え……？』じゃない！\r\nコールよ、コール。",
            "enText": "Not \"huh\"! Do the chant!",
            "nextBlock": 7,
            "origClipLength": 59,
            "voiceIdx": 4,
            "voiceSheetId": "051061001",
            "animData": [
                {
                    "origLen": 70,
                    "pathId": 3719225288802292086
