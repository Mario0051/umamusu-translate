            "nextBlock": 4,
            "origClipLength": 66,
            "voiceIdx": 54,
            "voiceSheetId": "051056001",
            "choices": [
                {
                    "jpText": "……ダメ",
                    "enText": "Yes, you do.",
                    "nextBlock": 4
