            "blockIdx": 1
        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "というわけで！　レッツ・バンジー☆\r\nですよ、トレーナーさん！",
            "enText": "Without further ado, let's bungee jump!",
            "nextBlock": 3,
            "origClipLength": 65,
            "voiceIdx": 3,
            "voiceSheetId": "051056001",
            "choices": [
                {
                    "jpText": "どういうわけで！？",
                    "enText": "I think there is much ado needed here!",
                    "nextBlock": 3
                }
