            "pathId": 392888654340427343,
            "blockIdx": 1
        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "って名前変わったりはしないですけどね。",
            "enText": "...Actually, on second thought, don't.",
            "nextBlock": 3,
            "origClipLength": 57,
            "voiceIdx": 14,
            "voiceSheetId": "051056001",
            "animData": [
                {
                    "origLen": 95,
                    "pathId": -6116480457978252982
