                {
                    "origLen": 76,
                    "pathId": 462693756960084137
                },
                {
                    "origLen": 48,
                    "pathId": 7364692314778402368
                }
            ],
            "pathId": 2958280885992936100,
            "blockIdx": 9
        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "かしこみ、かしこみ、ほんじゃまげ～！\r\n大殺界よ～、来るな～、絶対来るな～！",
            "enText": "Okely dokely, hoppity-mag! Havoc, I<n>cast ye out,\nya nag!",
            "nextBlock": 11,
            "origClipLength": 69,
            "voiceIdx": 103,
            "voiceSheetId": "051056001",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -7684240754364800898
                },
                {
                    "origLen": 80,
                    "pathId": 483202308867165211
                },
                {
                    "origLen": 42,
                    "pathId": -3428077146788500382
                }
            ],
            "pathId": -8212710916461524532,
            "blockIdx": 10
        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "ふんぎゃろ～、ふんぎゃろろ～……！\r\n大殺界よ、ゲットアウトー！！",
            "enText": "Fragalo! Fragalolo! Vile havoc, crawl<n>back in your\nhole!",
            "nextBlock": 12,
            "origClipLength": 66,
            "voiceIdx": 31,
            "voiceSheetId": "051056001",
            "animData": [
                {
                    "origLen": 77,
                    "pathId": -7843864774662412067
