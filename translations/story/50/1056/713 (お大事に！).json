    "text": [
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "まさかケガしちゃうなんて……\r\nこれはマズいですよ……トレーナーさん……。",
            "enText": "To think I'd injure myself... This doesn't<n>bode well\nat all, Trainer...",
            "nextBlock": 2,
            "origClipLength": 69,
            "voiceIdx": 18,
            "voiceSheetId": "051056001",
            "choices": [
                {
                    "jpText": "そんなにひどいのか……",
                    "enText": "Is it that bad?",
                    "nextBlock": 2
                },
                {
                    "jpText": "そんなにひどいんだ……",
                    "enText": "Is it that bad?",
                    "nextBlock": 2
                }
