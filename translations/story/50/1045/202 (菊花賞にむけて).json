            "pathId": 1737681349514289938,
            "blockIdx": 7
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "ん～、実力……。",
            "enText": "Power, huh...",
            "nextBlock": 9,
            "origClipLength": 55,
            "voiceIdx": 34,
            "voiceSheetId": "051045001",
            "animData": [
                {
                    "origLen": 66,
                    "pathId": 4184245360154469651
                },
                {
                    "origLen": 26,
                    "pathId": -6450736874054322767
