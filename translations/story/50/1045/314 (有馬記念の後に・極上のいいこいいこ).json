        {
            "jpName": "観客B",
            "enName": "Spectator B",
            "jpText": "俺も甘やかしてー。",
            "enText": "You can coddle me anytime!",
            "nextBlock": 13,
            "origClipLength": 56,
            "voiceIdx": -1,
