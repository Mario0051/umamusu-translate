            "blockIdx": 16
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "……がまん、がまんっ。",
            "enText": "Fight the urge, fight the urge!",
            "nextBlock": 18,
            "origClipLength": 59,
            "voiceIdx": 11,
            "voiceSheetId": "051045001",
            "animData": [
                {
                    "origLen": 70,
