            "pathId": 8753256972683418617,
            "blockIdx": 21
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "はい♪",
            "enText": "Me neither! ♪",
            "nextBlock": 23,
            "origClipLength": 48,
            "voiceIdx": 47,
            "voiceSheetId": "051045001",
            "animData": [
                {
                    "origLen": 59,
                    "pathId": 6093445279131906050
