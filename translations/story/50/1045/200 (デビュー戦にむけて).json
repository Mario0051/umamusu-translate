            "pathId": 1150524362402322045,
            "blockIdx": 10
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "どうしよう～、\r\n急にドキドキしてきちゃいました。",
            "enText": "My heart's pounding all of a sudden.<n>What should\nI do?",
            "nextBlock": 12,
            "origClipLength": 77,
            "voiceIdx": 36,
            "voiceSheetId": "051045001",
            "choices": [
                {
                    "jpText": "走り、楽しみにしてるから！",
                    "enText": "Remember that I'm looking forward to seeing\nyou run!",
                    "nextBlock": 12
                }
            ],
            "animData": [
                {
                    "origLen": 88,
                    "pathId": 29955076911722901
