            "blockIdx": 1
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "デビュー戦ですよ？　トレーナーさんの。\r\n私、もう胸がいっぱいで～。",
            "enText": "This is your debut race. My heart feels<n>like it\nmight just burst!",
            "nextBlock": 3,
            "origClipLength": 89,
            "voiceIdx": 1,
            "voiceSheetId": "051045001",
            "animData": [
                {
                    "origLen": 100,
