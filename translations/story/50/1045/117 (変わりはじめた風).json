            "pathId": 7694260793119228718,
            "blockIdx": 1
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "ふふっ。\r\nがおお～、食べちゃうぞ～♪",
            "enText": "Hehe! Grrrr! I'll eat you up! ♪",
            "nextBlock": 3,
            "origClipLength": 69,
            "voiceIdx": 3,
            "voiceSheetId": "051045001",
