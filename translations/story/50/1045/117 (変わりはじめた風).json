            "pathId": -9169934861373871652,
            "blockIdx": 2
        },
        {
            "jpName": "女の子A",
            "enName": "Girl A",
            "jpText": "きゃっきゃ！",
            "enText": "Eeeeep!",
            "nextBlock": 4,
            "origClipLength": 52,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 63,
                    "pathId": 3189734249962370591
