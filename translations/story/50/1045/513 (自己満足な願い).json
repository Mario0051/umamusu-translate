            "pathId": 3760677562218191344,
            "blockIdx": 5
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "いいえ、そんなんじゃありません。",
            "enText": "Oh, I don't know about that.",
            "nextBlock": 7,
            "origClipLength": 65,
            "voiceIdx": 3,
            "voiceSheetId": "051045001",
            "animData": [
                {
                    "origLen": 76,
