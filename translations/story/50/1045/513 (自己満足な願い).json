            "blockIdx": 15
        },
        {
            "jpName": "熱血トレーナー",
            "enName": "Passionate Trainer",
            "jpText": "ここで諦めないで！\r\nあなたの願いを言ってみなさい！",
            "enText": "You can't give up now! Tell me what it is<n>you hope\nfor!",
            "nextBlock": 17,
            "origClipLength": 78,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 89,
                    "pathId": 8617300509539752874
