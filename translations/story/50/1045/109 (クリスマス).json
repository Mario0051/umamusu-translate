            "pathId": 3820335044877933223,
            "blockIdx": 4
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "……？\r\nどうかしましたか？",
            "enText": "...Is something wrong?",
            "nextBlock": 6,
            "origClipLength": 63,
            "voiceIdx": 32,
            "voiceSheetId": "051045001",
            "choices": [
                {
                    "jpText": "ミスマッチだったから……",
                    "enText": "I had something else in mind...",
                    "nextBlock": 6
                }
