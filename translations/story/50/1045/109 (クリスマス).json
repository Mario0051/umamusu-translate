            "animData": [
                {
                    "origLen": 74,
                    "pathId": 34885893416822589
                },
                {
                    "origLen": 56,
                    "pathId": -5851527453194549418
                }
            ],
            "pathId": -3475524059311605734,
            "blockIdx": 5
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "あ、すみません。\r\nあまりに寒かったので。",
            "enText": "Oh, I'm sorry. It's just that it's so cold<n>now.",
            "nextBlock": 7,
            "origClipLength": 72,
            "voiceIdx": 0,
            "voiceSheetId": "051045001",
            "animData": [
