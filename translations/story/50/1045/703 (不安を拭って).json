                {
                    "origLen": 54,
                    "pathId": 7244144885549599123
                }
            ],
            "pathId": -8771878480317045526,
            "blockIdx": 7
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "……トレーナーさん。",
            "enText": "...Trainer.",
            "nextBlock": 9,
            "origClipLength": 58,
            "voiceIdx": 52,
            "voiceSheetId": "051045001",
            "choices": [
                {
                    "jpText": "こんな時間にどうしたんだ？",
                    "enText": "What are you up to at this hour?",
                    "nextBlock": 9
