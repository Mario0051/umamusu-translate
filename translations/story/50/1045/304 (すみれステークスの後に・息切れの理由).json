            "nextBlock": 19,
            "origClipLength": 77,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "俺の？",
                    "enText": "My job?",
                    "nextBlock": 19
                },
