            "origClipLength": 90,
            "voiceIdx": 74,
            "voiceSheetId": "051045001",
            "choices": [
                {
                    "jpText": "ご褒美をもらうようなことじゃ……",
                    "enText": "I'm just doing my job...",
                    "nextBlock": 5
