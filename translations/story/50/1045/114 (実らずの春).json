    "title": "実らずの春",
    "enTitle": "A Fruitless Spring",
    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "数日前――",
            "enText": "A few days ago...",
            "nextBlock": 2,
            "origClipLength": 51,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 63,
                    "pathId": -41980397568655204
