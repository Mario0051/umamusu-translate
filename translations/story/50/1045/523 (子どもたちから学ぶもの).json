            "enName": "",
            "jpText": "(ゴンッ！！)",
            "enText": "<i>Thunk!</i>",
            "nextBlock": 8,
            "origClipLength": 54,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "ク、クリークー！？",
                    "enText": "C-Creek?!",
                    "nextBlock": 8
