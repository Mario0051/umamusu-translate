            "pathId": -3966675889980489834,
            "blockIdx": 7
        },
        {
            "jpName": "客の男性",
            "enName": "Male Fan",
            "jpText": "ひっ！　そ、そうですか……すみません……。",
            "enText": "Oh... I-I see... Sorry...",
            "nextBlock": 9,
            "origClipLength": 72,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "……行っちゃったな",
                    "enText": "...He left.",
                    "nextBlock": 9
                },
                {
                    "jpText": "……行っちゃったね",
                    "enText": "...He left.",
                    "nextBlock": 9
