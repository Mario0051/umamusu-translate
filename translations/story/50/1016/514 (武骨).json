            "origClipLength": 63,
            "voiceIdx": 99,
            "voiceSheetId": "051016001",
            "choices": [
                {
                    "jpText": "何してるんだ？",
                    "enText": "What's going on?",
                    "nextBlock": 3
                },
                {
                    "jpText": "何してるの？",
                    "enText": "What's going on?",
                    "nextBlock": 3
