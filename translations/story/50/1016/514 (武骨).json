            "pathId": 29063897140148539,
            "blockIdx": 8
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "だからどうした。",
            "enText": "And?",
            "nextBlock": 10,
            "origClipLength": 55,
            "voiceIdx": 15,
            "voiceSheetId": "051016001",
            "animData": [
                {
                    "origLen": 66,
                    "pathId": -4362460001651082717
