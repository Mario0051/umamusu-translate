            "blockIdx": 1
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（――ガラガラガラッ！）",
            "enText": "<i>Rattle, rattle, rattle!</i>",
            "nextBlock": 3,
            "origClipLength": 60,
            "voiceIdx": -1,
