            "pathId": -6376746043672791590,
            "blockIdx": 9
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "…………。",
            "enText": "...",
            "nextBlock": 11,
            "origClipLength": 51,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 62,
