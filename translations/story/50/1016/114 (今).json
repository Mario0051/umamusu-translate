            "blockIdx": 1
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "…………。",
            "enText": "...",
            "nextBlock": 3,
            "origClipLength": 51,
            "voiceIdx": 79,
            "voiceSheetId": "051016001",
            "choices": [
                {
                    "jpText": "ブライアン？",
                    "enText": "What's up?",
                    "nextBlock": 3
