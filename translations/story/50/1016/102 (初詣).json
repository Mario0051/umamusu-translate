            "pathId": 7561727860843412454,
            "blockIdx": 17
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "では私は帰る。",
            "enText": "All right, I'm going home.",
            "nextBlock": 19,
            "origClipLength": 54,
            "voiceIdx": 44,
            "voiceSheetId": "051016001",
            "choices": [
                {
                    "jpText": "もう帰るのか？",
                    "enText": "Already?",
                    "nextBlock": 19
                },
                {
                    "jpText": "もう帰るの？",
                    "enText": "Already?",
                    "nextBlock": 19
                }
