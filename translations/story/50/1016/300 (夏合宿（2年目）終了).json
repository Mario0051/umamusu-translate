            "pathId": -5953683127963292920,
            "blockIdx": 21
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "？　何かあったのか？",
            "enText": "Did something happen?",
            "nextBlock": 23,
            "origClipLength": 58,
            "voiceIdx": 20,
            "voiceSheetId": "051017001",
            "choices": [
                {
                    "jpText": "彼女の体調が戻ったら、一緒に走ってほしい",
                    "enText": "When she fully recovers, I'd like you to run with\nher.",
                    "nextBlock": 23
                }
            ],
            "animData": [
