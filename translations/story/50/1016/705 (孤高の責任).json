            "pathId": -2050191450570363971,
            "blockIdx": 3
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "んん……ないか。",
            "enText": "Mmm... nothing here.",
            "nextBlock": 5,
            "origClipLength": 55,
            "voiceIdx": 34,
            "voiceSheetId": "051016001",
            "choices": [
                {
                    "jpText": "さっきから大丈夫？",
                    "enText": "You sure you're okay?",
                    "nextBlock": 5
                }
            ],
            "animData": [
                {
                    "origLen": 202,
                    "pathId": 6457284172040431189
                },
                {
                    "origLen": 157,
                    "pathId": -2089564483541901292
                }
