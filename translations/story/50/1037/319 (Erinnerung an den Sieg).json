    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "たくさんのレースを走り抜いてきたある日――",
            "enText": "たくさんのレースを走り抜いてきたある日――",
            "nextBlock": 2,
            "origClipLength": 72,
