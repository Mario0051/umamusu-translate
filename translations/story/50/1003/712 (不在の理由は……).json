        },
        {
            "jpName": "トウカイテイオー",
            "enName": "Tokai Teio",
            "jpText": "あ、トレーナー！\r\nこれはね～、気合い注入なんだっ！",
            "enText": "Oh, Trainer! I come here to pump myself<n>up!",
            "nextBlock": 5,
            "origClipLength": 61,
            "voiceIdx": 3,
            "voiceSheetId": "051003001",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 2213527205185214829
                },
                {
                    "origLen": 106,
                    "pathId": -2284068430537769403
