            "pathId": -7169587871660479468,
            "blockIdx": 2
        },
        {
            "jpName": "トウカイテイオー",
            "enName": "Tokai Teio",
            "jpText": "ぜーったい！\r\nカイチョーみたいになってやる～！！",
            "enText": "I'll become just like Prez! I swear I will!",
            "nextBlock": 4,
            "origClipLength": 61,
            "voiceIdx": 11,
            "voiceSheetId": "051003001",
            "choices": [
                {
                    "jpText": "弱音じゃない……？",
                    "enText": "Not here to vent?",
                    "nextBlock": 4
                }
            ],
            "animData": [
                {
                    "origLen": 89,
                    "pathId": -4629331062524675886
                },
                {
                    "origLen": 124,
                    "pathId": 6606128868922331201
