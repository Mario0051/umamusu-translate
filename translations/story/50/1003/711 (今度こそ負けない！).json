            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "なら今やるべきことはひとつ",
                    "enText": "Then there's only one thing to do.",
                    "nextBlock": 13
