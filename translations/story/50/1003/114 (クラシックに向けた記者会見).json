            "origClipLength": 69,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 80,
                    "pathId": 4608608611828617107
                },
                {
                    "origLen": 44,
                    "pathId": -7176761819295638408
                }
