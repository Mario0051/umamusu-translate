            "origClipLength": 61,
            "voiceIdx": 56,
            "voiceSheetId": "051003001",
            "choices": [
                {
                    "jpText": "忘れ物。そっちは？",
                    "enText": "I forgot something. How about you?",
                    "nextBlock": 7
