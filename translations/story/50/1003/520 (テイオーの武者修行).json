            "blockIdx": 9
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Umamusume A",
            "jpText": "そうだけど、貴方は？",
            "enText": "Yeah. And you are...",
            "nextBlock": 11,
            "origClipLength": 51,
            "voiceIdx": -1,
            "voiceSheetId": "",
