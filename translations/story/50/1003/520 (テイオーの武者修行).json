            "origClipLength": 69,
            "voiceIdx": 61,
            "voiceSheetId": "051003001",
            "animData": [
                {
                    "origLen": 80,
                    "pathId": -5020580716072220483
                },
                {
                    "origLen": 28,
                    "pathId": 3077638720974530239
                }
