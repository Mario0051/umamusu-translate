            "pathId": -8626038986526442094,
            "blockIdx": 7
        },
        {
            "jpName": "トウカイテイオー",
            "enName": "Tokai Teio",
            "jpText": "……じゃん？",
            "enText": "...You know?",
            "nextBlock": 9,
            "origClipLength": 48,
            "voiceIdx": -1,
