            "pathId": 6733642778879300832,
            "blockIdx": 7
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "ほら、見たまえ、あそこを――！",
            "enText": "See? Lo, over yonder!",
            "nextBlock": 9,
            "origClipLength": 64,
            "voiceIdx": 2,
            "voiceSheetId": "051015001",
