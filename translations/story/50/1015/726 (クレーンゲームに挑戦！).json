        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "さあ、幕を開けようじゃないか！",
            "enText": "Now, raise the curtain!",
            "nextBlock": 10,
            "origClipLength": 64,
            "voiceIdx": 91,
            "voiceSheetId": "051015001",
