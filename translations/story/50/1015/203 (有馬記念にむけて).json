            "pathId": 547640766511446138,
            "blockIdx": 10
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "聴け、ウマ娘を愛する観衆たちよ！",
            "enText": "Hear me, spectators whose hearts beat<n>with\nadoration for Umamusume!",
            "nextBlock": 12,
            "origClipLength": 65,
            "voiceIdx": 70,
            "voiceSheetId": "051015001",
            "animData": [
                {
                    "origLen": 76,
