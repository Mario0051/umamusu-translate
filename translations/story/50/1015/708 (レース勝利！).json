            "blockIdx": 10
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "んなっ！？",
            "enText": "What?!",
            "nextBlock": 12,
            "origClipLength": 51,
            "voiceIdx": 4,
            "voiceSheetId": "051015001",
