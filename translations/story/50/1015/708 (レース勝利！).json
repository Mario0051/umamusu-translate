        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "……………ふっ。\nふふふふっ…………はははっ。",
            "enText": "...Heh! Hehehe! ...Hahaha!",
            "nextBlock": 13,
            "origClipLength": 75,
            "voiceIdx": 2,
            "voiceSheetId": "051015001",
