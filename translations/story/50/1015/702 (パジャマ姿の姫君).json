            "blockIdx": 2
        },
        {
            "jpName": "パジャマ姿の女の子",
            "enName": "Girl in Pajamas",
            "jpText": "あっ、あの……あの……。",
            "enText": "Um... Uh... I...",
            "nextBlock": 4,
            "origClipLength": 60,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "（パジャマ姿の……子ども？）",
                    "enText": "(A child in pajamas? Alone out here?)",
                    "nextBlock": 4
                }
            ],
            "animData": [
                {
                    "origLen": 153,
                    "pathId": -1231683768257940989
                },
                {
                    "origLen": 71,
                    "pathId": -4675157890395338619
