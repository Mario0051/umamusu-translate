            "pathId": 550637924869454945,
            "blockIdx": 14
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "ボクは君の光～♪\r\nボクは君の道しるべ～♪",
            "enText": "I am your shining light. ♪ I am your<n>guiding\nlight. ♪",
            "nextBlock": 16,
            "origClipLength": 72,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 83,
                    "pathId": -7491254899133891434
                },
                {
                    "origLen": 47,
                    "pathId": -5947365265238253237
                }
