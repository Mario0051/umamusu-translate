    "text": [
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "ある日、テイエムオペラオーと\n廊下を歩いていると――",
            "enText": "One day, as I'm walking down the<n>hallway with\nT.M. Opera O...",
            "nextBlock": 2,
            "origClipLength": 78,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 89,
                    "pathId": 3436425344799141470
