            "origClipLength": 86,
            "voiceIdx": 2,
            "voiceSheetId": "051015001",
            "choices": [
                {
                    "jpText": "どうした、テイエムオペラオー？",
                    "enText": "Hey, what's wrong?",
                    "nextBlock": 6
                },
                {
                    "jpText": "どうしたの、テイエムオペラオー？",
                    "enText": "Hey, what's wrong?",
