                },
                {
                    "origLen": 50,
                    "pathId": 1756177240276410386
                }
            ],
            "pathId": 839479012316053882,
            "blockIdx": 11
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "は、はあ……？",
            "enText": "Oh... Math...",
            "nextBlock": 13,
            "origClipLength": 54,
            "voiceIdx": 49,
            "voiceSheetId": "051058001",
            "animData": [
                {
                    "origLen": 103,
                    "pathId": -2988964679316341351
                },
                {
                    "origLen": 65,
                    "pathId": -1463574538473901677
