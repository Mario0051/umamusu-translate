            "pathId": -6521106071299937708,
            "blockIdx": 2
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "す、すぐトレーニングに……いっっ！！",
            "enText": "I promise I'll be back to training as soon<n>\nas—Ouch!",
            "nextBlock": 4,
            "origClipLength": 68,
            "voiceIdx": 109,
            "voiceSheetId": "051058001",
            "choices": [
                {
                    "jpText": "痛むなら止めておこう",
                    "enText": "If you're in pain, let's call it a day.",
                    "nextBlock": 4
                }
            ],
            "animData": [
                {
                    "origLen": 72,
                    "pathId": -4617093501842736524
                },
                {
                    "origLen": 79,
                    "pathId": -3504951361537018717
