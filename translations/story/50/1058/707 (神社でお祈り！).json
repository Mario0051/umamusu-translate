            "pathId": 3930380513664102185,
            "blockIdx": 2
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "すぐ戻ってきますので……！",
            "enText": "I won't be too long!",
            "nextBlock": 4,
            "origClipLength": 76,
            "voiceIdx": 53,
            "voiceSheetId": "051058001",
