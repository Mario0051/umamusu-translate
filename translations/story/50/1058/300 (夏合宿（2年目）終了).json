            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": 2996877753407297020,
            "blockIdx": 1
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "あううううぅっ、すみません～～～。",
            "enText": "Uugh, I'm so sorry about this...",
            "nextBlock": 3,
            "origClipLength": 81,
            "voiceIdx": 65,
            "voiceSheetId": "051058001",
