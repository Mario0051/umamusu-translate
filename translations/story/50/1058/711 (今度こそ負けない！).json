            "nextBlock": 10,
            "origClipLength": 58,
            "voiceIdx": 76,
            "voiceSheetId": "051058001",
            "choices": [
                {
                    "jpText": "気持ちで負けるな！",
                    "enText": "Don't lose to your emotions!",
                    "nextBlock": 10
                },
                {
                    "jpText": "気持ちで負けないで！",
                    "enText": "Don't lose to your emotions!",
                    "nextBlock": 10
