            "blockIdx": 25
        },
        {
            "jpName": "観客",
            "enName": "Spectators",
            "jpText": "おいおい、大丈夫か……？",
            "enText": "Hey... will she be all right?",
            "nextBlock": 27,
            "origClipLength": 60,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 71,
                    "pathId": -2965982604198227445
                },
                {
                    "origLen": 71,
                    "pathId": -5501269270418721154
                }
            ],
            "pathId": 7083940116967192876,
            "blockIdx": 26
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "すー……はぁー…………！",
            "enText": "Breathe in... and... out!",
            "nextBlock": 28,
            "origClipLength": 60,
            "voiceIdx": 59,
            "voiceSheetId": "051058001",
            "animData": [
                {
                    "origLen": 71,
