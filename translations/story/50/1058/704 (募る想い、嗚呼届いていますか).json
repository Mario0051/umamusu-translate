            "pathId": 5708837592488399594,
            "blockIdx": 23
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ザワザワザワザワ……）",
            "enText": "<i>Chatter, chatter...</i>",
            "nextBlock": 25,
            "origClipLength": 60,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 71,
