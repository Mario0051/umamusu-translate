            ],
            "pathId": -2028834328462800224,
            "blockIdx": 22
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "ふばぁっ……！？",
            "enText": "<i>Sputter!</i>",
            "nextBlock": 24,
            "origClipLength": 99,
            "voiceIdx": 25,
            "voiceSheetId": "051058001",
            "animData": [
