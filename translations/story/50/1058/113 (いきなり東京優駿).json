        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ウオオオオオオオッ！！！！！）",
            "enText": "Wooooo!",
            "nextBlock": 16,
            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
