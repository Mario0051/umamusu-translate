            "pathId": 6477960892227183168,
            "blockIdx": 9
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "ぐす……っ、トレーナーさん。\r\n私、もっともっとがんばります。",
            "enText": "<i>Sniffle...</i> Trainer, I promise to work even<n>harder.",
            "nextBlock": 11,
            "origClipLength": 85,
            "voiceIdx": 69,
            "voiceSheetId": "051058001",
            "animData": [
                {
                    "origLen": 96,
                    "pathId": -4546739136168328310
                },
                {
                    "origLen": 53,
                    "pathId": -4532391806591940319
