            "blockIdx": 4
        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "お待たせしました！　入場用のハレ衣装を\r\nまとって、堂々の登場ですよー！",
            "enText": "Sorry for keeping you waiting. I wanted<n>to dress\nup in something flashy to make<n>a grand entrance!",
            "nextBlock": 6,
            "origClipLength": 91,
            "voiceIdx": 53,
            "voiceSheetId": "051056001",
            "animData": [
                {
                    "origLen": 102,
                    "pathId": 8754897974709254402
