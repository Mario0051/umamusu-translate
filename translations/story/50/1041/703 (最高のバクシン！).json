            "pathId": 27417674410960058,
            "blockIdx": 4
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "大丈夫！！\r\n今の貴方なら最高のバクシンができますよ！",
            "enText": "You got this! Show us your best<n>Bakushin!",
            "nextBlock": 6,
            "origClipLength": 80,
            "voiceIdx": 96,
            "voiceSheetId": "051041001",
            "pathId": 7285447255692725323,
            "blockIdx": 5
        },
        {
            "jpName": "そばかす顔のウマ娘",
            "enName": "Freckled Umamusume",
            "jpText": "うん！\r\nバクシンバクシン、だよねっ！",
            "enText": "Yes! Bakushin, BAKUSHIIIN! Right?",
