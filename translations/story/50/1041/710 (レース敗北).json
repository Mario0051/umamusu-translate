            "origClipLength": 52,
            "voiceIdx": 17,
            "voiceSheetId": "051041001",
            "choices": [
                {
                    "jpText": "失敗は悪いことじゃない",
                    "enText": "Failure isn't necessarily a bad thing.",
                    "nextBlock": 5
                },
                {
                    "jpText": "学級委員長、しっかり！",
                    "enText": "Hang in there, Class Representative!",
                    "nextBlock": 9
