            "origClipLength": 69,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "じゃあ次は、『葵ステークス』だ！",
                    "enText": "Then next up is the Aoi Stakes!",
                    "nextBlock": 17
