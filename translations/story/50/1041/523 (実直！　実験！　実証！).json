            "blockIdx": 14
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "限界を……超える～～！！",
            "enText": "I'll... surpass my limits!",
            "nextBlock": 16,
            "origClipLength": 105,
            "voiceIdx": 126,
            "voiceSheetId": "051041001",
            "animData": [
                {
                    "origLen": 58,
                    "pathId": -4243966450284306203
