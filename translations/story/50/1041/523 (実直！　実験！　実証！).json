            "pathId": -5038924277389031843,
            "blockIdx": 7
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "むむ！？\r\nでん……じゅし……メラ……！？",
            "enText": "Err?! Unique... radio... mela?! What?",
            "nextBlock": 9,
            "origClipLength": 72,
            "voiceIdx": 30,
            "voiceSheetId": "051041001",
            "animData": [
                {
                    "origLen": 83,
                    "pathId": -3737390197702780847
