            "origClipLength": 69,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 80,
                    "pathId": 6554585053086321254
                },
                {
                    "origLen": 53,
                    "pathId": -112463745195985741
                }
            ],
            "pathId": -9195803698817006759,
            "blockIdx": 15
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
