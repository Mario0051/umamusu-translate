            "pathId": -6837956145335485498,
            "blockIdx": 14
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "いよいよお待ちかね！\r\nトレーナーさんとトレーニングです！",
            "enText": "We can finally start today's training!",
            "nextBlock": 16,
            "origClipLength": 63,
            "voiceIdx": 48,
            "voiceSheetId": "051041001",
            "animData": [
                {
                    "origLen": 74,
                    "pathId": -4804583558355155294
