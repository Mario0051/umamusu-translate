            "blockIdx": 29
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "なんと！？\r\nでは、一体……！？",
            "enText": "It's not?! ...Then just what could it be?!",
            "nextBlock": 31,
            "origClipLength": 55,
            "voiceIdx": 25,
            "voiceSheetId": "051041001",
            "choices": [
                {
                    "jpText": "“スピード”だ！",
                    "enText": "It's speed!",
                    "nextBlock": 31
                },
                {
                    "jpText": "“スピード”だよ！",
                    "enText": "It's speed!",
                    "nextBlock": 31
                }
            ],
            "animData": [
                {
                    "origLen": 66,
                    "pathId": 7299978681433701637
                },
