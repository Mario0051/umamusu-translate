            "pathId": -1787603879820173873,
            "blockIdx": 11
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "それはですねッ！",
            "enText": "Well, you see!",
            "nextBlock": 13,
            "origClipLength": 50,
            "voiceIdx": 3,
            "voiceSheetId": "051041001",
            "animData": [
                {
                    "origLen": 61,
