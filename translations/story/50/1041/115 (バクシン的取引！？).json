            "pathId": 4305981781149090253,
            "blockIdx": 21
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "あれッ！？\r\nみなさんは！？",
            "enText": "Huh?! Where is everyone?!",
            "nextBlock": 23,
            "origClipLength": 54,
            "voiceIdx": 26,
            "voiceSheetId": "051041001",
            "choices": [
                {
                    "jpText": "行っちゃったぞ",
                    "enText": "They left.",
                    "nextBlock": 23
                },
                {
                    "jpText": "行っちゃったよ",
                    "enText": "They left.",
                    "nextBlock": 23
                }
