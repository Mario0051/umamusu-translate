            "nextBlock": 24,
            "origClipLength": 61,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "トレーナー室にあるぞ",
                    "enText": "I've got a copy in my office.",
                    "nextBlock": 24
                },
                {
                    "jpText": "トレーナー室にあるよ",
                    "enText": "I've got a copy in my office.",
                    "nextBlock": 24
