            "pathId": -2371381478141976585,
            "blockIdx": 12
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "なんでしたっけ！？",
            "enText": "Wait, what was it again?!",
            "nextBlock": 14,
            "origClipLength": 50,
            "voiceIdx": 20,
            "voiceSheetId": "051041001",
            "animData": [
                {
                    "origLen": 61,
