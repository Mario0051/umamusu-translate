            "origClipLength": 71,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "今はまだ、君は成長過程なんだ",
                    "enText": "This is just a step in the process.",
                    "nextBlock": 20
