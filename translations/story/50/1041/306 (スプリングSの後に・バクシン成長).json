            "pathId": 734948222952712277,
            "blockIdx": 27
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "ハイッ――",
            "enText": "Yes!",
            "nextBlock": 29,
            "origClipLength": 48,
            "voiceIdx": 48,
            "voiceSheetId": "051041001",
            "animData": [
