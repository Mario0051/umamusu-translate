            "pathId": -5435106629447393770,
            "blockIdx": 8
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "アアッ、大忙し大忙し！\r\n優等生は大変です！",
            "enText": "Ahhh, busy, busy! It's not easy being a<n>model\nstudent!",
            "nextBlock": 10,
            "origClipLength": 59,
            "voiceIdx": 59,
            "voiceSheetId": "051041001",
            "choices": [
                {
                    "jpText": "……トレーニングは？",
                    "enText": "...What about training?",
                    "nextBlock": 10
