    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "今日から『夏合宿』――\r\n実力向上のため強化トレーニングが始まる。",
            "enText": "Today is the first day of summer camp.<n>We'll be\nstarting a more rigorous routine<n>to further\nimprove performance.",
            "nextBlock": 2,
            "origClipLength": 66,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": 112304691851539742,
            "blockIdx": 1
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "そこーッ！\r\n早く荷物を部屋に運んでください！",
            "enText": "You there! Hurry and take your bags to<n>your\nroom!",
