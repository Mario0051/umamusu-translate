            "pathId": -7324057736228187811,
            "blockIdx": 8
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "うんうん、優等生らしいレース選びです！",
            "enText": "It's a race befitting a model student!",
            "nextBlock": 10,
            "origClipLength": 57,
            "voiceIdx": 153,
            "voiceSheetId": "051041001",
            "animData": [
                {
                    "origLen": 68,
                    "pathId": -4863255032803739323
                },
                {
                    "origLen": 35,
                    "pathId": -3921035613937365359
                }
            ],
            "pathId": -3576066251616822752,
            "blockIdx": 9
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "わかりました、\r\n次は『スプリングS』ですね！",
            "enText": "I understand! I'll join the Spring Stakes<n>next!",
            "nextBlock": 11,
            "origClipLength": 60,
            "voiceIdx": 47,
            "voiceSheetId": "051041001",
            "animData": [
                {
                    "origLen": 71,
                    "pathId": -7720203471831035224
