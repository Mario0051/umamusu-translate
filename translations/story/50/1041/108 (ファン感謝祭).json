            "nextBlock": 10,
            "origClipLength": 60,
            "voiceIdx": 2,
            "voiceSheetId": "051041001",
            "choices": [
                {
                    "jpText": "というと？",
                    "enText": "And how do you plan to do that?",
                    "nextBlock": 10
