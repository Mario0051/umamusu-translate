            "origClipLength": 85,
            "voiceIdx": 14,
            "voiceSheetId": "051013001",
            "choices": [
                {
                    "jpText": "君は食べないのか？",
                    "enText": "You're not gonna eat any?",
                    "nextBlock": 11
                },
                {
                    "jpText": "貴方は食べないの？",
                    "enText": "You're not gonna eat any?",
