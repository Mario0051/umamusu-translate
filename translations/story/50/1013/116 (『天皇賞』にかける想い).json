            "blockIdx": 13
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "すると、その途中――",
            "enText": "On the way there...",
            "nextBlock": 15,
            "origClipLength": 51,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
