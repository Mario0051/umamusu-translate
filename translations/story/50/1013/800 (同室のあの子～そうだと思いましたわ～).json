    "title": "同室のあの子～そうだと思いましたわ～",
    "enTitle": "My Roommate's Concern",
    "text": [
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "ふぅ……。",
            "enText": "<i>Sigh...</i>",
            "nextBlock": 2,
            "origClipLength": 85,
            "voiceIdx": 60,
            "voiceSheetId": "051013001",
            "animData": [
                {
                    "origLen": 39,
                    "pathId": 6453643029198082229
