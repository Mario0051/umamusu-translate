            "pathId": -7292869971459946928,
            "blockIdx": 23
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "え……そうですか？",
            "enText": "Oh... Really?",
            "nextBlock": 25,
            "origClipLength": 56,
            "voiceIdx": 20,
            "voiceSheetId": "051013001",
            "animData": [
                {
                    "origLen": 67,
