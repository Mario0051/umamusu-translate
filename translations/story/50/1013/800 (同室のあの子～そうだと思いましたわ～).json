            "blockIdx": 12
        },
        {
            "jpName": "イクノディクタス",
            "enName": "Ikuno Dictus",
            "jpText": "メジロマックイーンさんにトレーナーさん。\n本日もお疲れ様です。",
            "enText": "Hello, Mejiro McQueen and her trainer. I<n>hope\nyour day has been going well.",
            "nextBlock": 14,
            "origClipLength": 85,
            "voiceIdx": 128,
            "voiceSheetId": "051063001",
            "animData": [
                {
                    "origLen": 125,
                    "pathId": 8601609337580431556
                },
                {
                    "origLen": 119,
                    "pathId": 7401996317790198172
                },
                {
                    "origLen": 89,
                    "pathId": 8551635654272624345
