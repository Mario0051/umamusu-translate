            "pathId": 7367530106420994155,
            "blockIdx": 25
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "",
            "jpText": "はっ……！　イクノさん！",
            "enText": "",
            "nextBlock": 27,
            "origClipLength": 52,
