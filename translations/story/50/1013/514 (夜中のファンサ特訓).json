            "origClipLength": 69,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": -268005718491085973,
            "blockIdx": 1
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "ピ、ピスピース……！",
            "enText": "P-peace, peeeace!",
