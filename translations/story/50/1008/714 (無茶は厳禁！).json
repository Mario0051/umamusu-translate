                {
                    "origLen": 70,
                    "pathId": -5059187519641378314
                }
            ],
            "pathId": -3188481822370217522,
            "blockIdx": 4
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "こんなケガくらいじゃ立ち止まれねぇんだ。",
            "enText": "No little injury like this is gonna stop me.",
            "nextBlock": 6,
            "origClipLength": 71,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "壊れたバイクじゃ走れない",
                    "enText": "Broken motorcycles can't go anywhere, you\nknow?",
                    "nextBlock": 6
                }
            ],
            "animData": [
                {
                    "origLen": 76,
                    "pathId": -6817264798030730991
                },
                {
                    "origLen": 82,
