            "pathId": -6459416437932597331,
            "blockIdx": 7
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "カッコよくなりてえ！",
            "enText": "To try and be cool!",
            "nextBlock": 9,
            "origClipLength": 58,
            "voiceIdx": 2,
            "voiceSheetId": "051008001",
            "choices": [
                {
                    "jpText": "同じでは……？",
                    "enText": "...Is it always the same?",
                    "nextBlock": 9
                },
                {
                    "jpText": "……ずっと同じ？",
                    "enText": "...Is it always the same?",
                    "nextBlock": 9
