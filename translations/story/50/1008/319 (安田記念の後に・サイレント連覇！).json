            ],
            "pathId": 96336750050057328,
            "blockIdx": 4
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "え？　安田記念連覇？",
            "enText": "Huh? Again?",
            "nextBlock": 6,
            "origClipLength": 58,
            "voiceIdx": 30,
            "voiceSheetId": "051008001",
            "choices": [
                {
                    "jpText": "忘れてた？",
                    "enText": "Did you forget?",
                    "nextBlock": 6
