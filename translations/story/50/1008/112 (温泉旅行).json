            "pathId": -3500593188712444250,
            "blockIdx": 5
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "そう、そいつだ！",
            "enText": "Right! That!",
            "nextBlock": 7,
            "origClipLength": 55,
            "voiceIdx": 73,
            "voiceSheetId": "051008001",
            "animData": [
                {
                    "origLen": 66,
                    "pathId": 504281932671239806
