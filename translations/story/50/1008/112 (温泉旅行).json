                {
                    "origLen": 74,
                    "pathId": 8847274152383577925
                }
            ],
            "pathId": -3916419697571893583,
            "blockIdx": 12
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（――カタンゴトン、ガタンゴトン）",
            "enText": "<i>Click clack... Click clack...</i>",
            "nextBlock": 14,
            "origClipLength": 67,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 113,
