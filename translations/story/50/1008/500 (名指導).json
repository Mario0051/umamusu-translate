            "origClipLength": 97,
            "voiceIdx": 53,
            "voiceSheetId": "051008001",
            "choices": [
                {
                    "jpText": "少し休憩しよう",
                    "enText": "Why don't you take a little break?",
                    "nextBlock": 3
                },
                {
                    "jpText": "少し休憩しない？",
                    "enText": "Why don't you take a little break?",
                    "nextBlock": 3
                }
