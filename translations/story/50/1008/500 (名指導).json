            "blockIdx": 3
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "ほら！　余裕、余裕！",
            "enText": "See? I've totally got this!",
            "nextBlock": 5,
            "origClipLength": 58,
            "voiceIdx": 91,
            "voiceSheetId": "051008001",
