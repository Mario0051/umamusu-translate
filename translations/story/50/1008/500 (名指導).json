            "nextBlock": 9,
            "origClipLength": 81,
            "voiceIdx": 53,
            "voiceSheetId": "051008001",
            "choices": [
                {
                    "jpText": "よし、トレーニングを続けるか！",
                    "enText": "All right! Let's keep going then!",
                    "nextBlock": 9
                },
                {
                    "jpText": "よし、トレーニングを続けましょう！",
                    "enText": "All right! Let's keep going then!",
                    "nextBlock": 9
