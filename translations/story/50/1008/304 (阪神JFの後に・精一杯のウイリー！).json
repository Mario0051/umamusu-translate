            "animData": [
                {
                    "origLen": 87,
                    "pathId": -2406133827755862556
                },
                {
                    "origLen": 37,
                    "pathId": 8384622305380781463
                }
            ],
            "pathId": 5488795167285475367,
            "blockIdx": 8
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "うっ……優しくすんなよ。そう言われっと\r\n強がりも言えなくなんだろ……。",
            "enText": "Urgh... Stop being so nice. How am I<n>even\nsupposed to respond to that?",
            "nextBlock": 10,
            "origClipLength": 91,
            "voiceIdx": 4,
            "voiceSheetId": "051008001",
            "animData": [
                {
                    "origLen": 96,
                    "pathId": 3820881169259617265
                },
                {
                    "origLen": 102,
                    "pathId": -489910716477662262
