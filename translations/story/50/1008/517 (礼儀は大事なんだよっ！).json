            "pathId": 3232858296525761785,
            "blockIdx": 7
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "我慢できねぇんだよっ！",
            "enText": "I wouldn't be able to take it!",
            "nextBlock": 9,
            "origClipLength": 59,
            "voiceIdx": 36,
            "voiceSheetId": "051008001",
            "animData": [
