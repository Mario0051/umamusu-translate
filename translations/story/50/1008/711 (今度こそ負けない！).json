            "nextBlock": 7,
            "origClipLength": 81,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "ウオッカの努力を知ってるから！",
                    "enText": "I know how hard you work!",
