            "pathId": -1623721147720333878,
            "blockIdx": 30
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "――よし、じゃ注文しようぜ。",
            "enText": "...All right. Let's order.",
            "nextBlock": 32,
            "origClipLength": 63,
            "voiceIdx": 69,
            "voiceSheetId": "051008001",
            "animData": [
                {
                    "origLen": 74,
                    "pathId": 3803035502117223580
