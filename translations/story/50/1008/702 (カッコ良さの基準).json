                {
                    "origLen": 13,
                    "pathId": 6305249710959489094
                }
            ],
            "pathId": 7157290272340965553,
            "blockIdx": 17
        },
        {
            "jpName": "スタッフA",
            "enName": "Store Clerk A",
            "jpText": "いつもの……ですか？",
            "enText": "The... usual?",
            "nextBlock": 19,
            "origClipLength": 58,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 69,
                    "pathId": 3587908798074545938
