            "origClipLength": 56,
            "voiceIdx": 60,
            "voiceSheetId": "051002001",
            "choices": [
                {
                    "jpText": "どうしたんだ？",
                    "enText": "Is something wrong?",
                    "nextBlock": 14
                },
                {
                    "jpText": "どうしたの？",
                    "enText": "Is something wrong?",
                    "nextBlock": 14
