            "pathId": -5371429950300096060,
            "blockIdx": 1
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "はっ、はっ、はっ……！",
            "enText": "<i>Huff... Puff...</i>",
