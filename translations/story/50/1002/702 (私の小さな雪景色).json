            "nextBlock": 7,
            "origClipLength": 53,
            "voiceIdx": 0,
            "voiceSheetId": "051002001",
            "choices": [
                {
                    "jpText": "気になるなら行ってみよう",
                    "enText": "Do you want to go take a look?",
