            "origClipLength": 66,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "勉強とか",
                    "enText": "What about studying?",
                    "nextBlock": 11
                },
                {
                    "jpText": "料理とか",
