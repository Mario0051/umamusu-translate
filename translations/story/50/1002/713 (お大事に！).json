    "title": "お大事に！",
    "enTitle": "Get Well Soon!",
    "text": [
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "すみません、トレーナーさん。\r\nすぐに走れるようになりますから。",
            "enText": "I'm sorry, Trainer. I'll be able to run<n>again before\nyou know it.",
            "nextBlock": 2,
            "origClipLength": 65,
            "voiceIdx": 65,
            "voiceSheetId": "051002001",
            "choices": [
                {
                    "jpText": "今日は安静にするように",
                    "enText": "It's fine. You should rest for today.",
                    "nextBlock": 2
                }
            ],
            "animData": [
                {
                    "origLen": 76,
