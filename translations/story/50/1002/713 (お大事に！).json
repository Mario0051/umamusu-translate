            "blockIdx": 8
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "……わかりました。",
            "enText": "...Okay.",
            "nextBlock": 10,
            "origClipLength": 85,
            "voiceIdx": 49,
            "voiceSheetId": "051002001",
            "animData": [
