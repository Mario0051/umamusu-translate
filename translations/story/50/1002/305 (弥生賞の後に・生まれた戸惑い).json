            "origClipLength": 59,
            "voiceIdx": 54,
            "voiceSheetId": "051002001",
            "animData": [
                {
                    "origLen": 60,
                    "pathId": 2974428885157261324
                },
                {
                    "origLen": 70,
                    "pathId": -4489834364034218286
