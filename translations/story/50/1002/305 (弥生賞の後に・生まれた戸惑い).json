        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "……すみません……。",
            "enText": "I'm sorry...",
            "nextBlock": 18,
            "origClipLength": 51,
            "voiceIdx": 65,
            "voiceSheetId": "051002001",
            "animData": [
