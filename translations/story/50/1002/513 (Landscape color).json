    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "サイレンススズカの元に、勝負服が届いた。",
            "enText": "Silence Suzuka's signature racewear has<n>arrived.",
            "nextBlock": 2,
            "origClipLength": 58,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 69,
                    "pathId": -4028022878071942912
