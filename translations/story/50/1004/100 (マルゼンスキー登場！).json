            "origClipLength": 95,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "coloredText": [
                {
                    "jpText": "マルゼンスキー",
                    "enText": ""
                }
            ],
            "pathId": 8933913851297546267,
            "blockIdx": 1
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "……はずだったのだが。",
            "enText": "Or that was our plan, at least...",
            "nextBlock": 3,
            "origClipLength": 59,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": -285328227324897131,
            "blockIdx": 2
        },
