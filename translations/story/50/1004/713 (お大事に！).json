    "title": "お大事に！",
    "enTitle": "Get Well Soon!",
    "text": [
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "あーあ、ドジっちゃった。",
            "enText": "I messed up...",
            "nextBlock": 2,
            "origClipLength": 60,
            "voiceIdx": 60,
            "voiceSheetId": "051004001",
            "animData": [
                {
                    "origLen": 71,
