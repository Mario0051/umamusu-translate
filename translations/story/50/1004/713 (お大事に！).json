            "origClipLength": 91,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "気にしなくていい",
                    "enText": "It's okay.",
                    "nextBlock": 6
                },
                {
                    "jpText": "気にしなくていいよ",
                    "enText": "It's okay.",
                    "nextBlock": 6
