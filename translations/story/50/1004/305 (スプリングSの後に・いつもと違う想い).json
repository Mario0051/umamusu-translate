            "pathId": -5727701508069483268,
            "blockIdx": 4
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "…………。",
            "enText": "...",
            "nextBlock": 6,
            "origClipLength": 51,
            "voiceIdx": 78,
            "voiceSheetId": "051004001",
            "choices": [
                {
                    "jpText": "マルゼンスキー……？",
                    "enText": "...Maruzensky?",
