                {
                    "origLen": 50,
                    "pathId": 5045562802340857991
                }
            ],
            "pathId": -1471852488167652514,
            "blockIdx": 13
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ピンポンパンポーン）",
            "enText": "<i>Ding, dong....</i>",
            "nextBlock": 15,
            "origClipLength": 59,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 70,
                    "pathId": -8128284571168704490
