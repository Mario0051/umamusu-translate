            "pathId": -7257550741570213070,
            "blockIdx": 16
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "ウズウズしちゃって、少しでも多く\n走りたい気分なんだもの！",
            "enText": "I'm itching to get out there and run like<n>a speed\ndemon!",
            "nextBlock": 18,
            "origClipLength": 84,
            "voiceIdx": 0,
            "voiceSheetId": "051004001",
            "choices": [
                {
                    "jpText": "楽しい気持ちが伝わってくるよ",
                    "enText": "Your excitement is definitely rubbing off on me!",
                    "nextBlock": 18
