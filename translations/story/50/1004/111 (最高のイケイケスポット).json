            "pathId": -7365865052482769842,
            "blockIdx": 9
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "はい、到着～！",
            "enText": "We're here!",
            "nextBlock": 11,
            "origClipLength": 54,
            "voiceIdx": 5,
            "voiceSheetId": "051004001",
            "choices": [
                {
                    "jpText": "レース場がイケイケスポット？",
                    "enText": "The racecourse is your \"fly spot\"?",
                    "nextBlock": 11
