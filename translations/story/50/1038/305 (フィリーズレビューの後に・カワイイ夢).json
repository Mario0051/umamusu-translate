            "pathId": -3274507980854647870,
            "blockIdx": 7
        },
        {
            "jpName": "カレンチャン",
            "enName": "Curren Chan",
            "jpText": "ううん、いいの。\r\n大丈夫。",
            "enText": "It's okay. I get it.",
            "nextBlock": 9,
            "origClipLength": 63,
            "voiceIdx": 50,
            "voiceSheetId": "051038001",
            "animData": [
                {
                    "origLen": 74,
                    "pathId": 3818364580420521522
