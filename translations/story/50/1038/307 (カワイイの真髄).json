            "animData": [
                {
                    "origLen": 114,
                    "pathId": -4557337846883757219
                },
                {
                    "origLen": 72,
                    "pathId": -6292029743226862917
                }
            ],
            "pathId": 1657053645224580735,
            "blockIdx": 22
        },
        {
            "jpName": "カレンチャン",
            "enName": "Curren Chan",
            "jpText": "うん。",
            "enText": "Yeah.",
            "nextBlock": 24,
            "origClipLength": 48,
            "voiceIdx": 82,
            "voiceSheetId": "051038001",
            "animData": [
                {
                    "origLen": 59,
                    "pathId": 4653096095948453485
