            "origClipLength": 250,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": -5981496153348275726,
            "blockIdx": 10
        },
        {
            "jpName": "カレンチャン",
            "enName": "Curren Chan",
