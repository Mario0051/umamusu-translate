            "voiceSheetId": "051006001",
            "choices": [
                {
                    "jpText": "まずは説明してくれ",
                    "enText": "At least give me an explanation first.",
                    "nextBlock": 4
                },
                {
                    "jpText": "まずは説明して",
                    "enText": "At least give me an explanation first.",
                    "nextBlock": 4
                }
            ],
            "animData": [
                {
