            "blockIdx": 4
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "どうだった？",
            "enText": "How was it?",
            "nextBlock": 6,
            "origClipLength": 48,
            "voiceIdx": 155,
            "voiceSheetId": "051006001",
            "choices": [
                {
                    "jpText": "上手いけどオグリらしさが足りない",
                    "enText": "Great, but lacking in Oguri-ness.",
