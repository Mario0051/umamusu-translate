            "origClipLength": 61,
            "voiceIdx": 35,
            "voiceSheetId": "051006001",
            "choices": [
                {
                    "jpText": "オグリが好きな振りつけはあるか？",
                    "enText": "Do you have a favorite dance move?",
                    "nextBlock": 8
                },
                {
                    "jpText": "オグリが好きな振りつけはある？",
                    "enText": "Do you have a favorite dance move?",
                    "nextBlock": 8
