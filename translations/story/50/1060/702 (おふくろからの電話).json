            "blockIdx": 4
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "もしもし、急にどしたの？",
            "enText": "Hi, Mom. You never call me like this.<n>What's up?",
            "nextBlock": 6,
            "origClipLength": 60,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 148,
                    "pathId": -5068555033365297267
