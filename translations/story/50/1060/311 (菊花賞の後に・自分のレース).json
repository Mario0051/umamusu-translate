            "origClipLength": 90,
            "voiceIdx": 67,
            "voiceSheetId": "051060001",
            "choices": [
                {
                    "jpText": "ああ！",
                    "enText": "Yeah!",
                    "nextBlock": 5
                },
                {
                    "jpText": "うん！",
                    "enText": "Yeah!",
                    "nextBlock": 5
                }
