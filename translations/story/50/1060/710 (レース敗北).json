            "pathId": -9101977519918266959,
            "blockIdx": 13
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "……はっ！？",
            "enText": "What?!",
            "nextBlock": 15,
            "origClipLength": 52,
            "voiceIdx": 26,
            "voiceSheetId": "051060001",
            "choices": [
                {
                    "jpText": "あ～っ、悔しいっ！！",
                    "enText": "Nooooo! You were <i>this</i> close! Aaarrrgh!",
                    "nextBlock": 15
