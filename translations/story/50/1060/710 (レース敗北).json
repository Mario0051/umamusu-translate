            "pathId": 3727946716409249241,
            "blockIdx": 14
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "ちょちょちょ、\r\nトレーナーさんっ！？",
            "enText": "Whoa, whoa, whoa! Hold on, Trainer!",
            "nextBlock": 16,
            "origClipLength": 69,
            "voiceIdx": 25,
            "voiceSheetId": "051060001",
            "animData": [
                {
                    "origLen": 80,
                    "pathId": 2093966400941700073
