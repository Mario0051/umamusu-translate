    "text": [
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "……ごめん、トレーナーさん。\r\n負けちゃった。",
            "enText": "...Sorry, Trainer. I lost.",
            "nextBlock": 2,
            "origClipLength": 75,
            "voiceIdx": 24,
            "voiceSheetId": "051060001",
            "animData": [
                {
                    "origLen": 86,
                    "pathId": -4377341658026874877
