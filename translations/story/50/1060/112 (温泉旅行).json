            "enText": "...Finally.",
            "nextBlock": 14,
            "origClipLength": 54,
            "voiceIdx": 7,
            "voiceSheetId": "051060001",
            "choices": [
                {
                    "jpText": "ああ、お疲れ！",
                    "enText": "That was a long trip.",
                    "nextBlock": 14
                },
                {
                    "jpText": "うん、お疲れ！",
                    "enText": "That was a long trip.",
                    "nextBlock": 14
