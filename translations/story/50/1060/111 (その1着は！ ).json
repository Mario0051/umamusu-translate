                {
                    "origLen": 52,
                    "pathId": 8070862165363012035
                }
            ],
            "pathId": 721191332756569061,
            "blockIdx": 23
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "ホント困っちゃうわー。\r\n……ふふ。",
            "enText": "It's embarrassing! ...Hehe.",
            "nextBlock": 25,
            "origClipLength": 68,
            "voiceIdx": 2,
            "voiceSheetId": "051060001",
