            "blockIdx": 29
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "いいんだよ、みんなトレーナーさんのこと\r\n気に入ってるんだから。",
            "enText": "Don't. Everyone likes you.",
            "nextBlock": 31,
            "origClipLength": 86,
            "voiceIdx": 2,
            "voiceSheetId": "051060001",
            "animData": [
