            "pathId": -6140893929166304952,
            "blockIdx": 25
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "……はぁ。",
            "enText": "<i>Sigh...</i>",
            "nextBlock": 27,
            "origClipLength": 58,
            "voiceIdx": 60,
            "voiceSheetId": "051060001",
