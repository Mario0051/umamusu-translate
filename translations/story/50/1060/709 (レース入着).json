            "origClipLength": 95,
            "voiceIdx": 28,
            "voiceSheetId": "051060001",
            "choices": [
                {
                    "jpText": "じゃあ、このままでいいの？",
                    "enText": "So... you're okay with where you are then?",
                    "nextBlock": 9
