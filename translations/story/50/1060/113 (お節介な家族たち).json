            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "本当にみんなのことが好きなんだな",
                    "enText": "You really love them, huh?",
                    "nextBlock": 31
                },
                {
                    "jpText": "本当にみんなのことが好きなんだね",
                    "enText": "You really love them, huh?",
                    "nextBlock": 31
