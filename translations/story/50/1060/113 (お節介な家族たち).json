            "origClipLength": 95,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 106,
                    "pathId": -9132835772040953618
                },
                {
                    "origLen": 78,
                    "pathId": 2961941935612115633
                }
            ],
            "pathId": -2377217845354566379,
            "blockIdx": 29
        },
        {
            "jpName": "",
            "enName": "",
