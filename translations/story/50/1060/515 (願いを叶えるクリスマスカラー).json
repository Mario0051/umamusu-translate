            "blockIdx": 27
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "はは……まったく、\r\nトレーナーさんには敵いませんわ。",
            "enText": "Haha... You know me so well...",
            "nextBlock": 29,
            "origClipLength": 80,
            "voiceIdx": 4,
            "voiceSheetId": "051060001",
