            "pathId": -3854898471109282991,
            "blockIdx": 6
        },
        {
            "jpName": "ゴールドシップ",
            "enName": "Gold Ship",
            "jpText": "本当かぁ……？",
            "enText": "Really?",
            "nextBlock": 8,
            "origClipLength": 54,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 65,
