            "pathId": -4072135918040035237,
            "blockIdx": 11
        },
        {
            "jpName": "ゴールドシップ",
            "enName": "Gold Ship",
            "jpText": "面白そうじゃねぇか！　オッケー任せろ！",
            "enText": "Oh, that sounds fun! Okay, let's do this!",
            "nextBlock": 13,
            "origClipLength": 69,
            "voiceIdx": 31,
            "voiceSheetId": "051007001",
            "animData": [
                {
                    "origLen": 80,
                    "pathId": -562364681202688396
