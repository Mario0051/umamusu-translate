                {
                    "origLen": 55,
                    "pathId": -4519958495340805864
                }
            ],
            "pathId": 7724476700356009540,
            "blockIdx": 13
        },
        {
            "jpName": "ゴールドシップ",
            "enName": "Gold Ship",
            "jpText": "…………。",
            "enText": "...",
            "nextBlock": 15,
            "origClipLength": 51,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 62,
