        },
        {
            "jpName": "ゴールドシップ",
            "enName": "Gold Ship",
            "jpText": "次はアタシも負けてらんねーぜ！！\r\nな、トレーナー！",
            "enText": "I won't lose the next one! Right, Trainer?",
            "nextBlock": 3,
            "origClipLength": 78,
            "voiceIdx": 53,
            "voiceSheetId": "051007001",
            "choices": [
                {
                    "jpText": "今日も十分よかった！",
                    "enText": "You did great today!",
                    "nextBlock": 3
                },
                {
                    "jpText": "負けてらんねーよ！",
                    "enText": "I know you won't!",
                    "nextBlock": 8
