            "nextBlock": 5,
            "origClipLength": 116,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "choices": [
                {
                    "jpText": "設定、変わってないか……？",
                    "enText": "Is it just me, or did you change her backstory?",
                    "nextBlock": 5
