            "blockIdx": 2
        },
        {
            "jpName": "ゴールドシップ",
            "enName": "Gold Ship",
            "jpText": "ああ、ダイエットか！",
            "enText": "Oh, you're on a <i>diet!</i>",
            "nextBlock": 4,
            "origClipLength": 58,
            "voiceIdx": 37,
            "voiceSheetId": "051007001",
            "animData": [
                {
                    "origLen": 69,
                    "pathId": -1592649450793055484
