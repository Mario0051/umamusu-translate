            "blockIdx": 3
        },
        {
            "jpName": "ゴールドシップ",
            "enName": "Gold Ship",
            "jpText": "なんでだよ……！！！！\r\nどうしてこんなことになったんだ！！！！",
            "enText": "Why?! Why does it have to be like this?!",
            "nextBlock": 5,
            "origClipLength": 86,
            "voiceIdx": 17,
            "voiceSheetId": "051007001",
            "choices": [
                {
                    "jpText": "ど、どうしたんだ！？",
                    "enText": "Wh-what's wrong?!",
                    "nextBlock": 5
                },
                {
                    "jpText": "ど、どうしたの！？",
                    "enText": "Wh-what's wrong?!",
                    "nextBlock": 5
                }
