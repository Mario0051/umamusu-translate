    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "ゴールドシップと初詣に\r\nやってきたのだが……。",
            "enText": "I join Gold Ship on our first shrine visit of<n>the new\nyear.",
            "nextBlock": 2,
            "origClipLength": 76,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 87,
                    "pathId": 9208801995975723059
