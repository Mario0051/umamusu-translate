        {
            "jpName": "？？？",
            "enName": "？？？",
            "jpText": "あら、納得のいっていない顔ですね。",
            "enText": "あら、納得のいっていない顔ですね。",
            "nextBlock": 9,
            "origClipLength": 67,
            "voiceIdx": 30,
            "voiceSheetId": "059006001",
