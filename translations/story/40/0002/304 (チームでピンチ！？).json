            "origClipLength": 94,
            "voiceIdx": 18,
            "voiceSheetId": "051056001",
            "choices": [
                {
                    "jpText": "ちょ、ちょっと待って！",
                    "enText": "ちょ、ちょっと待って！",
                    "nextBlock": 10
