            "pathId": 864483794270138070,
            "blockIdx": 11
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "道を開けなさい！　キングが通るわっ！！",
            "enText": "Clear the way! I'm coming through!",
            "nextBlock": 13,
            "origClipLength": 87,
            "voiceIdx": 10,
            "voiceSheetId": "041061004",
            "animData": [
                {
                    "origLen": 50,
                    "pathId": 814440712222933297
