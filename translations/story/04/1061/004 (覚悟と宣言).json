            "blockIdx": 19
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "むぅぅぅぅぅぅぅっ！",
            "enText": "Hnnngh!",
            "nextBlock": 21,
            "origClipLength": 58,
            "voiceIdx": 18,
            "voiceSheetId": "041061004",
            "animData": [
                {
                    "origLen": 16,
                    "pathId": -3352797194070652463
                },
                {
                    "origLen": 82,
                    "pathId": -100675311028447017
