            "blockIdx": 28
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "……あら？",
            "enText": "...Huh?",
            "nextBlock": 30,
            "origClipLength": 21,
            "voiceIdx": 23,
            "voiceSheetId": "061061003",
            "animData": [
                {
