                {
                    "origLen": 73,
                    "pathId": -8350231768515671186
                }
            ],
            "pathId": 3881106884028536538,
            "blockIdx": 25
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "とっ、とととトレーナーなんとかしなさいよ～～！！！",
            "enText": "T-T-Trainer! Hurry up and do something!",
            "nextBlock": 27,
            "origClipLength": 144,
            "voiceIdx": 20,
            "voiceSheetId": "061061003",
            "animData": [
                {
                    "origLen": 17,
