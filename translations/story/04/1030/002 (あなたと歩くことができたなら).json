            "nextBlock": 14,
            "origClipLength": 201,
            "voiceIdx": 8,
            "voiceSheetId": "041030002",
            "choices": [
                {
                    "jpText": "君もそれを望んでくれるなら",
                    "enText": "Only if that's what you want too.",
                    "nextBlock": 14
