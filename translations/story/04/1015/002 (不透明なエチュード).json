            "blockIdx": 22
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（チャリーン！）",
            "enText": "<i>Clink!</i>",
            "nextBlock": 24,
            "origClipLength": 55,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 20,
