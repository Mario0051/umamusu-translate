            "pathId": -4481460979226611779,
            "blockIdx": 20
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "なんてことだ！　元トレーナー君じゃないか！",
            "enText": "What fortune is this?! If it isn't my former<n>provisional trainer!",
            "nextBlock": 22,
            "origClipLength": 140,
            "voiceIdx": 8,
            "voiceSheetId": "041015004",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 6083261879098006919
                },
                {
                    "origLen": 86,
                    "pathId": 3536433583730059249
                }
