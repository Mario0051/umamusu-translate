            "origClipLength": 168,
            "voiceIdx": 2,
            "voiceSheetId": "041041004",
            "choices": [
                {
                    "jpText": "つまりバッチリ寝てきたんだな！",
                    "enText": "Sounds like you got a good night's sleep!",
                    "nextBlock": 4
