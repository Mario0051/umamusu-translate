            "blockIdx": 1
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "ふぅ……ふぅ……はぁ～……。",
            "enText": "<i>Huff, huff.</i> Phew...",
            "nextBlock": 3,
            "origClipLength": 137,
            "voiceIdx": 0,
            "voiceSheetId": "041045003",
            "animData": [
                {
                    "origLen": 119,
