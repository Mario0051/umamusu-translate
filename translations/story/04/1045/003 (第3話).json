    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "いつものようにスーパークリークと\r\nトレーニングをしていたある日――",
            "enText": "One day, while training with Super Creek<n>as usual...",
            "nextBlock": 2,
