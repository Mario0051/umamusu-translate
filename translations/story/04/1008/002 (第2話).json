    "text": [
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "はぁっ、はぁ――うっし、インターバル走終わりっと！",
            "enText": "<i>Pant, pant...</i> That's interval training done!",
            "nextBlock": 2,
            "origClipLength": 210,
            "voiceIdx": 0,
            "voiceSheetId": "041008002",
            "animData": [
                {
                    "origLen": 42,
                    "pathId": -4419631922664135069
