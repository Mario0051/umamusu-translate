            "nextBlock": 8,
            "origClipLength": 276,
            "voiceIdx": 4,
            "voiceSheetId": "061035002",
            "choices": [
                {
                    "jpText": "店長さんのこと、知ってるのか？",
                    "enText": "You know the owner?",
                    "nextBlock": 8
                },
                {
                    "jpText": "店長さんのこと、知ってるの？",
                    "enText": "You know the owner?",
                    "nextBlock": 8
