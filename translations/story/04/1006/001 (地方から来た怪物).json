            "origClipLength": 95,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "pathId": -7975368227258627799,
            "blockIdx": 18
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "む……？",
