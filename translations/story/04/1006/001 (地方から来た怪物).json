            "pathId": -2539377727984515918,
            "blockIdx": 28
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "ううん……。",
            "enText": "Ummm...",
            "nextBlock": 30,
            "origClipLength": 55,
