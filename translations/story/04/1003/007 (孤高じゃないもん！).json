            "pathId": 6855747236856905001,
            "blockIdx": 26
        },
        {
            "jpName": "トウカイテイオー",
            "enName": "Tokai Teio",
            "jpText": "トレーナー……。",
            "enText": "Trainer...",
            "nextBlock": 28,
            "origClipLength": 41,
