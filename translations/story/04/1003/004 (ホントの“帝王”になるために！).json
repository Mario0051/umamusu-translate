            "pathId": 2110494962903467153,
            "blockIdx": 3
        },
        {
            "jpName": "トウカイテイオー",
            "enName": "Tokai Teio",
            "jpText": "あ、けど……そっか。",
            "enText": "No, wait... Mm...",
            "nextBlock": 5,
            "origClipLength": 125,
            "voiceIdx": 2,
            "voiceSheetId": "041003004",
