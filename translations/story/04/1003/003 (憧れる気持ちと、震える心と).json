            "origClipLength": 127,
            "voiceIdx": 3,
            "voiceSheetId": "041003003",
            "choices": [
                {
                    "jpText": "調子はどうだ？",
                    "enText": "How are you feeling?",
                    "nextBlock": 7
                },
                {
                    "jpText": "調子はどう？",
                    "enText": "How are you feeling?",
                    "nextBlock": 7
