            "pathId": -8268867217674609841,
            "blockIdx": 14
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "――780,500",
            "enText": "Score: 780,500",
            "nextBlock": 16,
            "origClipLength": 56,
            "voiceIdx": -1,
