            "origClipLength": 309,
            "voiceIdx": 16,
            "voiceSheetId": "061003002",
            "animData": [
                {
                    "origLen": 92,
                    "pathId": 2078091460016556248
                }
            ],
            "pathId": 4559761852529590009,
            "blockIdx": 20
        },
        {
            "jpName": "女の子",
            "enName": "Girl",
            "jpText": "おねーちゃん、がんばってー！",
            "enText": "You can do it!",
            "nextBlock": 22,
            "origClipLength": 68,
            "voiceIdx": 17,
            "voiceSheetId": "061003002",
