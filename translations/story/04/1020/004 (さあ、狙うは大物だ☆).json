            "pathId": -5155303604334791797,
            "blockIdx": 13
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "………………。",
            "enText": "...",
            "nextBlock": 15,
            "origClipLength": 69,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 13,
