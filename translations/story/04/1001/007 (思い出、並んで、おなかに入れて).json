            "pathId": 78674410914602325,
            "blockIdx": 1
        },
        {
            "jpName": "スペシャルウィーク",
            "enName": "Special Week",
            "jpText": "うへぇ～……あ、暑いぃ。",
            "enText": "Blegh... I-it's so hot...",
            "nextBlock": 3,
            "origClipLength": 160,
            "voiceIdx": 0,
            "voiceSheetId": "061001003",
            "animData": [
                {
                    "origLen": 162,
                    "pathId": -2393220435576549064
