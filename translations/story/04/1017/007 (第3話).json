            "animData": [
                {
                    "origLen": 19,
                    "pathId": -4174686579173270089
                },
                {
                    "origLen": 78,
                    "pathId": -3583642757524397184
                }
            ],
            "pathId": -2200093076610023085,
            "blockIdx": 11
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "すぐ行こう！　事件でない事を祈る！！",
            "enText": "Let's go! I pray nothing is wrong!",
            "nextBlock": 13,
            "origClipLength": 91,
            "voiceIdx": 9,
            "voiceSheetId": "061017003",
            "animData": [
                {
                    "origLen": 114,
