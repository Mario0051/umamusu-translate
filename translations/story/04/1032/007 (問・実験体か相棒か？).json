            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 53,
                    "pathId": 8450026742564008054
                }
            ],
            "pathId": 4061453925589346774,
            "blockIdx": 40
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "そして、翌日――",
            "enText": "The next day...",
            "nextBlock": 42,
            "origClipLength": 55,
            "voiceIdx": -1,
