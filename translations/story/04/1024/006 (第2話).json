            "pathId": -6706162418639303080,
            "blockIdx": 4
        },
        {
            "jpName": "マヤノの友だちB",
            "enName": "Maya's Friend B",
            "jpText": "出かける？　って……どこに？",
            "enText": "Head out? Where?",
            "nextBlock": 6,
            "origClipLength": 102,
            "voiceIdx": 3,
            "voiceSheetId": "061024002",
            "animData": [
                {
                    "origLen": 194,
                    "pathId": -2131769533695044237
