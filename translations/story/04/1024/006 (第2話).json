            "origClipLength": 70,
            "voiceIdx": 4,
            "voiceSheetId": "061024002",
            "animData": [
                {
                    "origLen": 194,
                    "pathId": 8566397865751469936
                }
            ],
            "pathId": 3649202152725812862,
            "blockIdx": 6
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "あっ……トレーナーちゃん。",
            "enText": "Oh... hi.",
            "nextBlock": 8,
            "origClipLength": 88,
            "voiceIdx": 5,
            "voiceSheetId": "061024002",
            "choices": [
                {
                    "jpText": "元気がないな、どうした？",
                    "enText": "You seem to be out of spirits. What's the\nmatter?",
                    "nextBlock": 8
                },
                {
                    "jpText": "元気ないね、どうしたの？",
                    "enText": "You seem to be out of spirits. What's the\nmatter?",
                    "nextBlock": 8
                }
            ],
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -4214711551865992407
                },
                {
                    "origLen": 58,
                    "pathId": 7352291626521723710
