            "pathId": -7079200744246976571,
            "blockIdx": 23
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "はっ……はっ……はっ……。",
            "enText": "<i>Huff... Puff...</i>",
            "nextBlock": 25,
            "origClipLength": 90,
            "voiceIdx": 15,
            "voiceSheetId": "041050004",
