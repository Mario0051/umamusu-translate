            "blockIdx": 18
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "よし……っ！",
            "enText": "I did it!",
            "nextBlock": 20,
            "origClipLength": 38,
            "voiceIdx": 7,
            "voiceSheetId": "041050005",
            "choices": [
                {
                    "jpText": "手ごたえがあったみたいだな",
                    "enText": "Looks like you've got the hang of it.",
