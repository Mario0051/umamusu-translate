            "nextBlock": 31,
            "origClipLength": 208,
            "voiceIdx": 14,
            "voiceSheetId": "041050005",
            "choices": [
                {
                    "jpText": "そうか！？それはよかった！",
                    "enText": "Really?! That's great!",
                    "nextBlock": 31
                },
                {
                    "jpText": "そう！？それはよかった！",
                    "enText": "Really?! That's great!",
                    "nextBlock": 31
                }
            ],
            "animData": [
                {
