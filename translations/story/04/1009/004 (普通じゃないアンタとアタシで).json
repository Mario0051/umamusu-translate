            "pathId": 5589085565441352980,
            "blockIdx": 7
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "はあ！？",
            "enText": "Huh?!",
            "nextBlock": 9,
            "origClipLength": 24,
            "voiceIdx": 5,
            "voiceSheetId": "041009004",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 1147844733583262664
