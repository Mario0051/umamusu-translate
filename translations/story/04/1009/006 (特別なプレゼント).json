    "text": [
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "休日、散歩がてら街を歩いていると――",
            "enText": "It's my day off, so I decide to go for a<n>stroll downtown when...",
            "nextBlock": 2,
            "origClipLength": 68,
            "voiceIdx": -1,
            "voiceSheetId": "",
