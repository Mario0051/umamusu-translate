            "pathId": 6734131666644532288,
            "blockIdx": 3
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "ふふっ、そうですね。",
            "enText": "Hehe, that sounds good.",
            "nextBlock": 5,
            "origClipLength": 81,
            "voiceIdx": 2,
            "voiceSheetId": "061002003",
            "animData": [
                {
                    "origLen": 20,
