            ],
            "pathId": 4991409761782245094,
            "blockIdx": 1
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "合宿も残すところあと数日……。",
            "enText": "There are only a few days left of camp...",
            "nextBlock": 3,
            "origClipLength": 87,
            "voiceIdx": 0,
            "voiceSheetId": "061002003",
            "animData": [
                {
                    "origLen": 99,
                    "pathId": 2514407590296567134
                },
                {
                    "origLen": 99,
                    "pathId": -7418946234001243444
                }
            ],
            "pathId": 2666127081753837157,
            "blockIdx": 2
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "トレーニングに集中できる環境なだけに、\n終わってしまうのが名残惜しいです。",
            "enText": "This really is the perfect environment for<n>me to focus on my training.\nI'll be sad to<n>bid it farewell.",
            "nextBlock": 4,
            "origClipLength": 176,
            "voiceIdx": 1,
            "voiceSheetId": "061002003",
            "choices": [
                {
                    "jpText": "残りの日も悔いなく過ごそう",
                    "enText": "Let's enjoy these last days to their fullest.",
                    "nextBlock": 4
                }
            ],
            "animData": [
                {
                    "origLen": 80,
                    "pathId": 6163003508979399567
                },
                {
                    "origLen": 187,
                    "pathId": -3058281727981302743
