            "blockIdx": 31
        },
        {
            "jpName": "スズカのトレーナー",
            "enName": "Suzuka's Trainer",
            "jpText": "スズカ？　……どうかしたの？",
            "enText": "Suzuka? What's wrong?",
            "nextBlock": 33,
            "origClipLength": 81,
            "voiceIdx": 23,
            "voiceSheetId": "041002002",
            "animData": [
                {
                    "origLen": 92,
                    "pathId": 3067917142333150888
