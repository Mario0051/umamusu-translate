                {
                    "origLen": 39,
                    "pathId": -854517987948830701
                }
            ],
            "pathId": -4636673773450627772,
            "blockIdx": 13
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "はっ……はっ……はっ……！！",
            "enText": "<I>Huff... huff... huff...</I>",
            "nextBlock": 15,
            "origClipLength": 111,
            "voiceIdx": 10,
            "voiceSheetId": "041002002",
            "animData": [
                {
                    "origLen": 156,
                    "pathId": 4303653461193351563
