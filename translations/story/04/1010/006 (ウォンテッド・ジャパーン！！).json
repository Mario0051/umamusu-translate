            "blockIdx": 3
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "マンホールのベストショット！\r\nフォトは角度が大切デスネ。",
            "enText": "The best possible shot of this manhole<n>cover! Photography's all about\nfindin' the<n>right angle.",
            "nextBlock": 5,
            "origClipLength": 180,
            "voiceIdx": 2,
            "voiceSheetId": "061010002",
            "choices": [
                {
                    "jpText": "写真撮ってたのか",
                    "enText": "Oh, you were taking a picture.",
                    "nextBlock": 5
                },
                {
                    "jpText": "写真撮ってたんだ",
                    "enText": "Oh, you were taking a picture.",
                    "nextBlock": 5
                }
            ],
            "animData": [
                {
                    "origLen": 52,
                    "pathId": 6239911722593548128
