            "pathId": 4301888546351685243,
            "blockIdx": 17
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "オーウ、ファミリーレストラン！！",
            "enText": "Ooh, a sit-down restaurant!",
            "nextBlock": 19,
            "origClipLength": 92,
            "voiceIdx": 11,
            "voiceSheetId": "041010004",
            "animData": [
                {
                    "origLen": 21,
                    "pathId": 7254599002982284646
