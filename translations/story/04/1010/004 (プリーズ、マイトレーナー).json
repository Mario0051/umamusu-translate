            "pathId": 2084811630573325342,
            "blockIdx": 2
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "うぅ……。\r\nうぅぅうぅ～……！",
            "enText": "<i>Sob...</i>",
            "nextBlock": 4,
            "origClipLength": 112,
            "voiceIdx": 0,
            "voiceSheetId": "041010004",
            "choices": [
                {
                    "jpText": "タイキ、遅くなってすまない！",
                    "enText": "Sorry I'm late, Taiki!",
                    "nextBlock": 4
                },
                {
                    "jpText": "タイキ、遅くなってごめん！",
                    "enText": "Sorry I'm late, Taiki!",
                    "nextBlock": 4
                }
