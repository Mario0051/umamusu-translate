            "blockIdx": 19
        },
        {
            "jpName": "美浦寮のウマ娘",
            "enName": "Miho Umamusume",
            "jpText": "ヒシアマ姐さん……。",
            "enText": "Hishiama...",
            "nextBlock": 21,
            "origClipLength": 47,
            "voiceIdx": 15,
            "voiceSheetId": "041012004",
            "animData": [
                {
                    "origLen": 239,
                    "pathId": 6172636198398291130
