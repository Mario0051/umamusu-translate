            "pathId": -2639950462438032603,
            "blockIdx": 1
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "ワン、ツー……フィニーッシュ♪",
            "enText": "One, two, and... finish! ♪",
            "nextBlock": 3,
            "origClipLength": 110,
            "voiceIdx": 0,
            "voiceSheetId": "061004001",
            "animData": [
                {
                    "origLen": 39,
