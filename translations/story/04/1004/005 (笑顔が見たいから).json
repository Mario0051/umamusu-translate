            "pathId": -8374592139411854468,
            "blockIdx": 8
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "(キーンコーン、カーンコーン……)",
            "enText": "<i>Ding, dong, ding, dong...</i>",
            "nextBlock": 10,
            "origClipLength": 138,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 131,
                    "pathId": -2000569840559835534
