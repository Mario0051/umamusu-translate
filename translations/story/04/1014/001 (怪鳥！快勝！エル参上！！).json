            "origClipLength": 254,
            "voiceIdx": 11,
            "voiceSheetId": "041014001",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -6101095111246337311
                },
                {
                    "origLen": 60,
                    "pathId": 8482376041368427627
                }
