            "pathId": 9061271774099348052,
            "blockIdx": 8
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "はいっ、トレーナーさん！　一緒に走りますよ！！",
            "enText": "Look alive, Trainer—we're goin' on a run!",
            "nextBlock": 10,
            "origClipLength": 119,
            "voiceIdx": 6,
            "voiceSheetId": "061014001",
            "choices": [
                {
                    "jpText": "走るのか！？",
                    "enText": "A run?! Right now?!",
                    "nextBlock": 10
                },
                {
                    "jpText": "走るの！？",
                    "enText": "A run?! Right now?!",
                    "nextBlock": 10
                }
