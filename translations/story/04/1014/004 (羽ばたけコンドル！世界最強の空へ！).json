            "pathId": -38757875995137073,
            "blockIdx": 1
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "不撓不屈のウマ娘、エルコンドルパサー！\r\nケガを乗り越え、華麗に登、場、デェェス！",
            "enText": "She's fearless, she's bold, she's El Condor<n>Pasa! Having overcome her\ninjury, she<n>makes her triumphant return to the ring!",
            "nextBlock": 3,
            "origClipLength": 261,
            "voiceIdx": 0,
            "voiceSheetId": "041014004",
            "choices": [
                {
                    "jpText": "おはよう",
                    "enText": "Good morning.",
                    "nextBlock": 3
                }
            ],
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -3562910001591124649
                },
                {
                    "origLen": 42,
                    "pathId": -3209180194290214957
