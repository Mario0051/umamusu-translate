            "pathId": -5541215559163602892,
            "blockIdx": 17
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "ふわわわ……ねむねむ……よろよろ……。\nエル、ふらふら……。",
            "enText": "Whew... Sleepy... Spinning... I'm... dizzy...",
            "nextBlock": 19,
            "origClipLength": 239,
            "voiceIdx": 11,
            "voiceSheetId": "041014002",
            "animData": [
                {
                    "origLen": 357,
                    "pathId": -939211645875229767
                },
                {
                    "origLen": 169,
                    "pathId": 8788549533971346897
                }
