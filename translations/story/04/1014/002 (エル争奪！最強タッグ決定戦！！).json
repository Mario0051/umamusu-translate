            "blockIdx": 20
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "ケッ！？",
            "enText": "Qué?!",
            "nextBlock": 22,
            "origClipLength": 52,
            "voiceIdx": 14,
            "voiceSheetId": "041014002",
            "animData": [
                {
                    "origLen": 47,
