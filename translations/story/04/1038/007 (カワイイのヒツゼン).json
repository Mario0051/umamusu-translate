            "nextBlock": 4,
            "origClipLength": 176,
            "voiceIdx": 1,
            "voiceSheetId": "041038007",
            "choices": [
                {
                    "jpText": "無理しない範囲で頑張ろう！",
                    "enText": "Let's just do as much as we can!",
                    "nextBlock": 4
