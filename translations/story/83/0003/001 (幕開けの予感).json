            "blockIdx": 1
        },
        {
            "jpName": "トウカイテイオー",
            "enName": "Tokai Teio",
            "jpText": "はっ、はっ、はっ、はっ……。\r\n――ふうっ。",
            "enText": "<i>Huff... Huff... Huff... Huff...</i> Phew!",
            "nextBlock": 3,
            "origClipLength": 83,
            "voiceIdx": 58,
            "voiceSheetId": "051003001",
            "animData": [
