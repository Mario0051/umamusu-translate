    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "トレーニングコースに来ると――",
            "enText": "I've come to the training track.",
            "nextBlock": 2,
            "origClipLength": 64,
            "voiceIdx": -1,
            "voiceSheetId": "",
