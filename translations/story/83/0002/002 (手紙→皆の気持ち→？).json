            "pathId": -1779201965010712569,
            "blockIdx": 2
        },
        {
            "jpName": "女の子A",
            "enName": "Girl A",
            "jpText": "もしかして……スズカさんですか？",
            "enText": "Are you... Suzuka?",
            "nextBlock": 4,
            "origClipLength": 65,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 76,
