            "pathId": -5153387931629413157,
            "blockIdx": 4
        },
        {
            "jpName": "女の子A",
            "enName": "Girl A",
            "jpText": "わぁ～！　ホンモノだぁ～！",
            "enText": "Oh my gosh! It's really you!",
            "nextBlock": 6,
            "origClipLength": 61,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 91,
                    "pathId": 4741011727580459772
