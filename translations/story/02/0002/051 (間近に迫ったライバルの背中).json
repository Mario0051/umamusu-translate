            "pathId": -7397549874096093525,
            "blockIdx": 25
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "記憶しておきます。",
            "enText": "I'll keep that in mind.",
            "nextBlock": 27,
            "origClipLength": 45,
            "voiceIdx": 21,
            "voiceSheetId": "020002051",
            "animData": [
                {
                    "origLen": 69,
                    "pathId": -5868098165042541331
                },
                {
                    "origLen": 127,
                    "pathId": -3177040714759744399
