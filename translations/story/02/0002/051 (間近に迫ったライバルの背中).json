            "pathId": 1457985104639183400,
            "blockIdx": 20
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "かしこまりました。",
            "enText": "I will.",
            "nextBlock": 22,
            "origClipLength": 44,
            "voiceIdx": 16,
            "voiceSheetId": "020002051",
            "animData": [
                {
                    "origLen": 30,
