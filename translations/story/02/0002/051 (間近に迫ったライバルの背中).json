            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "私でよければ、なんでも聞いてくれ。",
            "enText": "If I can be of help, I shall gladly answer.",
            "nextBlock": 29,
            "origClipLength": 83,
            "voiceIdx": 23,
            "voiceSheetId": "020002051",
            "animData": [
                {
                    "origLen": 127,
                    "pathId": -3071884619124951258
                },
                {
                    "origLen": 54,
                    "pathId": -7012031414659219534
                }
            ],
            "pathId": 725237689530951367,
            "blockIdx": 28
        },
