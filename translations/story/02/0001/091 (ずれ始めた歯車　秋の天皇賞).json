            "pathId": 7381763461171811217,
            "blockIdx": 25
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "…………。",
            "enText": "...",
            "nextBlock": 27,
            "origClipLength": 43,
            "voiceIdx": 15,
            "voiceSheetId": "020001091",
            "choices": [
                {
                    "jpText": "マックイーン？",
                    "enText": "McQueen?",
                    "nextBlock": 27
