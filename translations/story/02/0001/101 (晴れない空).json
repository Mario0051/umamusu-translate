            "pathId": -1352703576813809048,
            "blockIdx": 11
        },
        {
            "jpName": "前髪ぱっつんのウマ娘",
            "enName": "Upbeat Umamusume",
            "jpText": "そうですっ！\n次のレースも期待してますから！",
            "enText": "That's right! I'll be looking forward to your<n>next race!",
            "nextBlock": 13,
            "origClipLength": 109,
            "voiceIdx": 9,
            "voiceSheetId": "020001101",
            "animData": [
                {
                    "origLen": 154,
                    "pathId": 4293906495239266938
