            "pathId": 802520278541850066,
            "blockIdx": 25
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "ケ……？　ここは？",
            "enText": "Huh? Where am I?",
            "nextBlock": 27,
            "origClipLength": 84,
            "voiceIdx": 24,
            "voiceSheetId": "090002004",
            "animData": [
                {
                    "origLen": 12,
                    "pathId": -3141188678091753374
                },
                {
                    "origLen": 12,
                    "pathId": -4167449001578521493
