            "blockIdx": 10
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "スペシャルウィークさん……？　あの、何か？",
            "enText": "Special Week? Is something the matter?",
            "nextBlock": 12,
            "origClipLength": 142,
            "voiceIdx": 10,
            "voiceSheetId": "090001005",
            "animData": [
                {
                    "origLen": 178,
                    "pathId": -6594343800693195182
