            "blockIdx": 9
        },
        {
            "jpName": "メジロドーベル",
            "enName": "Mejiro Dober",
            "jpText": "な、投げ飛ばす？",
            "enText": "H-hurl? A boulder?",
            "nextBlock": 11,
            "origClipLength": 77,
            "voiceIdx": 9,
            "voiceSheetId": "090004003",
            "animData": [
                {
                    "origLen": 68,
