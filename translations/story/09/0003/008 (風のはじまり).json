                {
                    "origLen": 50,
                    "pathId": -4594927065501621765
                }
            ],
            "pathId": -4823118271320782611,
            "blockIdx": 19
        },
        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "……！",
            "enText": "...!",
            "nextBlock": 21,
            "origClipLength": 32,
            "voiceIdx": 19,
            "voiceSheetId": "090003008",
            "animData": [
                {
                    "origLen": 60,
