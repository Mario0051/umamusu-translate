                }
            ],
            "pathId": 1324220534660026346,
            "blockIdx": 13
        },
        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "え……？",
            "enText": "Huh?",
            "nextBlock": 15,
            "origClipLength": 38,
            "voiceIdx": 13,
            "voiceSheetId": "090003008",
            "animData": [
                {
                    "origLen": 89,
                    "pathId": 6055555149260081136
