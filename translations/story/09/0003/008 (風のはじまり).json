                },
                {
                    "origLen": 146,
                    "pathId": -8162130370323204835
                }
            ],
            "pathId": 268284505230777175,
            "blockIdx": 1
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "キタちゃん……。",
            "enText": "Kita...",
            "nextBlock": 3,
            "origClipLength": 50,
            "voiceIdx": 1,
            "voiceSheetId": "090003008",
            "animData": [
                {
                    "origLen": 168,
                    "pathId": 4011422092101972582
                },
                {
                    "origLen": 94,
                    "pathId": -2992163731562875947
