                {
                    "origLen": 123,
                    "pathId": 4039426058849092695
                }
            ],
            "pathId": -5655338928588538228,
            "blockIdx": 17
        },
        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "夢、実力、それに……ライバル。",
            "enText": "Dreams, skills... and rivals.",
            "nextBlock": 19,
            "origClipLength": 152,
            "voiceIdx": 17,
            "voiceSheetId": "090003008",
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 4289516124078917384
                },
                {
                    "origLen": 60,
                    "pathId": -5359208784879784688
                },
                {
                    "origLen": 317,
                    "pathId": -8971320910010440254
                },
                {
                    "origLen": 155,
