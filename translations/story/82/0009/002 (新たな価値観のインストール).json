            "pathId": 2378943823420017037,
            "blockIdx": 14
        },
        {
            "jpName": "ニシノフラワー",
            "enName": "Nishino Flower",
            "jpText": "……も、もう……ダメ……ですぅ～～。",
            "enText": "I-I can't... keep up...",
            "nextBlock": 16,
            "origClipLength": 68,
            "voiceIdx": 28,
            "voiceSheetId": "051051001",
            "animData": [
                {
                    "origLen": 42,
