            "blockIdx": 4
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "――ノンノン！　そこはすぐにスマイル！\r\nさっきから同じところでクワイエットですヨ？",
            "enText": "Hey now, show me a smile! You keep<n>gettin' all\nquiet!",
            "nextBlock": 6,
            "origClipLength": 72,
            "voiceIdx": 51,
            "voiceSheetId": "051010001",
            "animData": [
                {
                    "origLen": 83,
                    "pathId": -1157155265083628040
