            "pathId": -3169357980077655691,
            "blockIdx": 12
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……アマさん。",
            "enText": "...Ama.",
            "nextBlock": 14,
            "origClipLength": 54,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 202,
                    "pathId": 8197762575961369083
                },
                {
                    "origLen": 80,
                    "pathId": 4712214760543093244
