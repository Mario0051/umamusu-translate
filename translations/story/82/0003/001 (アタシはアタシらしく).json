            "pathId": -3101531875890655288,
            "blockIdx": 2
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Umamusume A",
            "jpText": "あ、姐さん……？",
            "enText": "Uh, Hishiama?",
            "nextBlock": 4,
            "origClipLength": 55,
            "voiceIdx": -1,
            "voiceSheetId": "",
            "animData": [
                {
                    "origLen": 202,
                    "pathId": -253189591196456288
