                }
            ],
            "pathId": -447409763321026762,
            "blockIdx": 25
        },
        {
            "jpName": "シンコウウインディ",
            "enName": "Shinko Windy",
            "jpText": "うりゃああ～～～！！",
            "enText": "GRAAAAAHHHHH!",
            "nextBlock": 27,
            "origClipLength": 84,
            "voiceIdx": 126,
            "voiceSheetId": "051043001",
            "animData": [
